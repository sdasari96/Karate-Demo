<script type="text/javascript">
$(document).ready(function(){
	
	if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ){
         $('.mobilenav').show();
		$('.fp-content').remove();
	}else if( /48.0 Firefox/i.test(navigator.userAgent) ){
		 
		$('.mobilenav').remove();
		$('.fp-content').show();
		$('.fp-content').removeClass('hidden');
	}else{
	     $('.main-form-content').show();
		 $('.fp-content').remove();
	}
});
</script>
<!-- Feature Phone Code END -->
 <script type = "text/javascript">
    function changeHashOnLoad() {
        window.location.href += "#";
        setTimeout("changeHashAgain()", "50");
        setTimeout("showTextP()","50");
    }
    
	function showTextP(){
		  $('.cartitemsPG p:gt(1)').show();  
		  $('.cartitemsPG p:gt(2)').hide();  
		
	}

    function changeHashAgain(){          
        window.location.href += "1";
    }

    var storedHash = window.location.hash;
    window.setInterval(function () {
        if (window.location.hash != storedHash) {
			//Defect 17664: this was causing scrolling issues in IE
			//when hash was being set to ''
			if(storedHash!=''){
				window.location.hash = storedHash;
			}
        }
    }, 50);     
     
    </script>
</body>
