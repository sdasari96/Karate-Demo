	$(document)
			.ready(
					function() {

						$("#split0").hide();
						$("#split1").hide();
						$("#split2").hide();
						$("#split3").hide();

						$("#splitable").hide();

						$("#addButtonsplit")
								.click(
										function() {

											if (document
													.getElementById('merchantData.split.submerchant').value == '') {

												return false;
											}

											counter++;
											$("#splitable").show();

											for (var i = 0; i < 10; i++) {

												if (document
														.getElementById('mid'
																+ i).value == ''
														|| document
																.getElementById('mid'
																		+ i).value == null) {

													var mid = document
															.getElementById('merchantData.split.submerchant').value;
													document
															.getElementById('mid'
																	+ i).value = mid;
													document
															.getElementById('merchantData.split.submerchant').value = "";

													var tid = document
															.getElementById('merchantData.split.subterminal').value;
													document
															.getElementById('tid'
																	+ i).value = tid;
													document
															.getElementById('merchantData.split.subterminal').value = "";

													var ref = document
															.getElementById('merchantData.split.txnref').value;
													document
															.getElementById('ref'
																	+ i).value = ref;
													document
															.getElementById('merchantData.split.txnref').value = "";

													var tamt = document
															.getElementById('merchantData.split.txnamt').value;
													document
															.getElementById('tamt'
																	+ i).value = tamt;
													document
															.getElementById('merchantData.split.txnamt').value = "";

													var agamt = document
															.getElementById('merchantData.split.aggamt').value;
													document
															.getElementById('agamt'
																	+ i).value = agamt;
													document
															.getElementById('merchantData.split.aggamt').value = "";

													var desc = document
															.getElementById('merchantData.split.txndesc').value;
													document
															.getElementById('desc'
																	+ i).value = desc;
													document
															.getElementById('merchantData.split.txndesc').value = "";

													var sudf1 = document
															.getElementById('merchantData.split.udf1').value;
													document
															.getElementById('sudf1'
																	+ i).value = sudf1;
													document
															.getElementById('merchantData.split.udf1').value = "";

													var sudf2 = document
															.getElementById('merchantData.split.udf2').value;
													document
															.getElementById('sudf2'
																	+ i).value = sudf2;
													document
															.getElementById('merchantData.split.udf2').value = "";

													var sudf3 = document
															.getElementById('merchantData.split.udf3').value;
													document
															.getElementById('sudf3'
																	+ i).value = sudf3;
													document
															.getElementById('merchantData.split.udf3').value = "";

													var sudf4 = document
															.getElementById('merchantData.split.udf4').value;
													document
															.getElementById('sudf4'
																	+ i).value = sudf4;
													document
															.getElementById('merchantData.split.udf4').value = "";

													var sudf5 = document
															.getElementById('merchantData.split.udf5').value;
													document
															.getElementById('sudf5'
																	+ i).value = sudf5;
													document
															.getElementById('merchantData.split.udf5').value = "";

													$("#split" + i).show();
													break;
												}

											}

											if (counter > 11) {

												return false;
											}

										});

					});

	$(document)
			.ready(
					function() {

						$("#row0").hide();
						$("#row1").hide();
						$("#row2").hide();
						$("#row3").hide();
						$("#row4").hide();
						$("#row5").hide();
						$("#row6").hide();
						$("#shoppingdetails").hide();

						$("#addButton")
								.click(
										function() {

											if (document
													.getElementById('merchantData.item.sno').value == '') {

												return false;
											}

											counter++;
											$("#shoppingdetails").show();

											for (var i = 0; i < 10; i++) {

												if (document
														.getElementById('itemno'
																+ i).value == ''
														|| document
																.getElementById('itemno'
																		+ i).value == null) {

													var itemno = document
															.getElementById('merchantData.item.sno').value;
													document
															.getElementById('itemno'
																	+ i).value = itemno;
													document
															.getElementById('merchantData.item.sno').value = "";

													var itemdesc = document
															.getElementById('merchantData.item.description').value;
													document
															.getElementById('itemdesc'
																	+ i).value = itemdesc;
													document
															.getElementById('merchantData.item.description').value = "";

													var itemcost = document
															.getElementById('merchantData.item.cost').value;
													document
															.getElementById('itemcost'
																	+ i).value = itemcost;
													document
															.getElementById('merchantData.item.cost').value = "";

													var itemquantity = document
															.getElementById('merchantData.item.quantity').value;
													document
															.getElementById('itemquantity'
																	+ i).value = itemquantity;
													document
															.getElementById('merchantData.item.quantity').value = "";

													$("#row" + i).show();
													break;
												}

											}

											if (counter > 11) {

												return false;
											}

										});

					});

	$(document)
			.ready(
					function() {

						$("#local0").hide();
						$("#local1").hide();
						$("#local2").hide();
						$("#local3").hide();
						$("#local").hide();

						$("#addButtonlocal")
								.click(
										function() {

											if (document
													.getElementById('merchantData.localnote.sno').value == '') {

												return false;
											}

											counter++;
											$("#local").show();

											for (var i = 0; i < 4; i++) {

												if (document
														.getElementById('localno'
																+ i).value == ''
														|| document
																.getElementById('localno'
																		+ i).value == null) {

													var localno = document
															.getElementById('merchantData.localnote.sno').value;
													document
															.getElementById('localno'
																	+ i).value = localno;
													document
															.getElementById('merchantData.localnote.sno').value = "";

													var localdesc = document
															.getElementById('merchantData.localnote.description').value;
													document
															.getElementById('localdesc'
																	+ i).value = localdesc;
													document
															.getElementById('merchantData.localnote.description').value = "";

													var localoption = document
															.getElementById('merchantData.localnote.paymentoption').value;
													document
															.getElementById('localoption'
																	+ i).value = localoption;
													document
															.getElementById('merchantData.localnote.paymentoption').value = "";

													$("#local" + i).show();
													break;
												}

											}

											if (counter > 11) {

												return false;
											}

										});

					});
	
	
	
	$(function() {
		$('#carddetailsdiv').hide();
		$('#netbankingdetailsdiv').hide();
		$('#cardtype').hide();
		$('#walletdiv').hide();
		$('#netbankingtype').hide();
		$('#wallettype').hide();
		$('#paymentmode')
				.change(
						function() {

							if (this.options[this.selectedIndex].value == 'DEBITCARD'
									|| this.options[this.selectedIndex].value == 'CREDITCARD') {

								document.getElementById("cardinfo.cardtype").value = this.options[this.selectedIndex].value;
								document.getElementById("netbanking.cardtype").value = "";
								document.getElementById("wallet.cardtype").value = "";

								$('#carddetailsdiv').show();
								$('#netbankingdetailsdiv').hide();
								$('#cardtype').show();
								$('#walletdiv').hide();
								$('#netbankingtype').hide();
								$('#wallettype').hide();

							}
							if (this.options[this.selectedIndex].value == 'select') {

								$('#carddetailsdiv').hide();
								$('#netbankingdetailsdiv').hide();
								$('#cardtype').hide();
								$('#walletdiv').hide();
								$('#netbankingtype').hide();
								$('#wallettype').hide();

							}

							if (this.options[this.selectedIndex].value == 'NETBANKING') {

								document.getElementById("cardinfo.cardtype").value = "";
								document.getElementById("netbanking.cardtype").value = this.options[this.selectedIndex].value;
								document.getElementById("wallet.cardtype").value = "";
								$('#carddetailsdiv').hide();
								$('#netbankingdetailsdiv').show();
								$('#cardtype').hide();
								$('#netbankingtype').show();
								$('#walletdiv').hide();
								$('#wallettype').hide();

							}

							if (this.options[this.selectedIndex].value == 'JIOMONEY') {

								document.getElementById("cardinfo.cardtype").value = "";
								document.getElementById("netbanking.cardtype").value = "";
								document.getElementById("wallet.cardtype").value = "";

								$('#carddetailsdiv').hide();
								$('#netbankingdetailsdiv').hide();
								$('#cardtype').hide();
								$('#walletdiv').hide();
								$('#netbankingtype').hide();
								$('#wallettype').hide();
							}
							if (this.options[this.selectedIndex].value == 'WALLETCASH') {

								document.getElementById("cardinfo.cardtype").value = "";
								document.getElementById("netbanking.cardtype").value = "";
								document.getElementById("wallet.cardtype").value = this.options[this.selectedIndex].value;

								$('#carddetailsdiv').hide();
								$('#netbankingdetailsdiv').hide();
								$('#cardtype').hide();
								$('#walletdiv').show();
								$('#netbankingtype').hide();
								$('#wallettype').show();

							}
						});

	});

	function changeurl() {
		var e = document.getElementById("refundversion");
		var selectedOption = e.options[e.selectedIndex].value;

		if (selectedOption == '1.0') {

			$('#refundinfodiv').show();
			$('#addinfo').hide();
			$('#format').hide();

			var c = document.getElementById("env");
			var env = c.options[c.selectedIndex].value;
			$
					.ajax({

						url : 'geturl',
						data : {
							env : env
						},
						type : 'POST',
						success : function(data) {

							var obj = JSON.parse(data);

							document.getElementById("refundserviceurl").value = obj[0].refund_service_url;

						},
						error : function(err) {

							console.log('error in ajax call');
						}
					});

		} else {

			$('#refundinfodiv').hide();
			$('#addinfo').show();
			$('#format').show();
			var c = document.getElementById("env");
			var env = c.options[c.selectedIndex].value;
			$
					.ajax({

						url : 'geturl',
						data : {
							env : env
						},
						type : 'POST',
						success : function(data) {

							var obj = JSON.parse(data);

							document.getElementById("refundserviceurl").value = obj[0].transac_service_url;

						},
						error : function(err) {

							console.log('error in ajax call');
						}
					});
		}
	}
	
	
	function changecollecturl()
	{
	var e = document.getElementById("collectversion");
	var selectedOption = e.options[e.selectedIndex].value;
	if (selectedOption == '4.0')
	{
	var c = document.getElementById("env");
	var env = c.options[c.selectedIndex].value;
	$
	.ajax({
	url : 'geturl',
	data : {
	env : env
	},
	type : 'POST',
	success : function(data) {
	var obj = JSON.parse(data);
	document.getElementById("collect_service_url").value = obj[0].collect_service_url;
	},
	error : function(err) {
	console.log('error in ajax call');
	}
	});
	}
	}

	function statusversionchange() {

		var e = document.getElementById("statusversion");
		var selectedOption = e.options[e.selectedIndex].value;

		if (selectedOption == '1.0') {

			$('#statustime').hide();
			$('#statustxntime').hide();

		} else {

			var date = new Date();
			/* var formattedDate = date.getFullYear()
					+ ("0" + (date.getMonth() + 1)).slice(-2)
					+ ("0" + date.getDate()).slice(-2) + (date.getHours())
					+ ("0" + date.getMinutes()).slice(-2)
					+ ("0" + date.getSeconds()).slice(-2); */
					
					function pad2(n) { 
						return n < 10 ? '0' + n : n 
					}

					var formattedDate = date.getFullYear().toString() + pad2(date.getMonth() + 1) + pad2( date.getDate()) + pad2( date.getHours() ) + pad2( date.getMinutes() ) + pad2( date.getSeconds() );


			$('#statustime').show();
			$('#statustxntime').hide();

			document.getElementById("statustimestamp").value = formattedDate;

		}
	}

	function transversionchange() {

		var t = document.getElementById("transversion");
		var selectedOption = t.options[t.selectedIndex].value;

		if (selectedOption == '1.0') {

			$('#statustime').hide();
			$('#statustxntime').hide();

		}
		if (selectedOption == '2.0') {

			$('#statustime').show();
			$('#statustxntime').hide();
		}

		if (selectedOption == '3.0' || selectedOption == '3.1' ||selectedOption == '3.3'|| selectedOption == '5.0') {

			$('#statustime').hide();
			$('#BillPay').hide();
			

			var date = new Date();
			/* var formattedDate = date.getFullYear()
					+ ("0" + (date.getMonth() + 1)).slice(-2)
					+ ("0" + date.getDate()).slice(-2) + (date.getHours())
					+ ("0" + date.getMinutes()).slice(-2)
					+ ("0" + date.getSeconds()).slice(-2); */
			
					function pad2(n) { 
						return n < 10 ? '0' + n : n 
					}

					var formattedDate = date.getFullYear().toString() + pad2(date.getMonth() + 1) + pad2( date.getDate()) + pad2( date.getHours() ) + pad2( date.getMinutes() ) + pad2( date.getSeconds() );


			$('#statustime').show();
			$('#statustxntime').show();

			document.getElementById("statustimestamp").value = formattedDate;

		}
	}

	$(function() {
		$('#errorMessage').hide();
		$('#message').hide();
		$('#transver').hide();
		$('#format').hide();
		$('#statustime').hide();
		$('#savecard').hide();

		$('#version').blur(function() {

			var selectedOption = document.getElementById("version").value

			/* 		var t = document.getElementById("version");
					var selectedOption = t.options[t.selectedIndex].value; */

			if (selectedOption == '2.1' || selectedOption == '2.3') {

				$('#savecard').show();
				$("#SIAutoPay").hide();

			} 
					else if(selectedOption=="3.3" || selectedOption=="5.0")
				{
				 $('#savecard').show();
				$("#SIAutoPay").show();
				
				}
					else
					{

						$('#savecard').hide();
						$("#SIAutoPay").hide();

					}
		});
		
		//BillPay
		
		$('#version').change(function() {

			var selectedOption = document.getElementById("version").value
			if (selectedOption == '2.1' || selectedOption == '2.3') {

				$('#savecard').show();
				$("#SIAutoPay").hide();

			} 
					else if(selectedOption=="3.3")
				{
				 $('#savecard').show();
				$("#SIAutoPay").show();
				
				}
					else
					{

						$('#savecard').hide();
						$("#SIAutoPay").hide();

					}
		});
		
	
		
		$('#refundversion').change(function() {

			changeurl();
			var selectedOption = document.getElementById("refundversion").value
			if(selectedOption!='1.0')
				{
				$('#refundUDF').show();
				}
			else
				{
				$('#refundUDF').hide();
				}

		});

		$('#txntype')
				.change(
						function() {

							var d = new Date().getTime();
							var uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'
									.replace(
											/[xy]/g,
											function(c) {
												var r = (d + Math.random() * 16) % 16 | 0;
												d = Math.floor(d / 16);
												return (c == 'x' ? r
														: (r & 0x3 | 0x8))
														.toString(16);
											});

							document.getElementById("starttime").value = "NA";
							document.getElementById("endtime").value = "NA";

							var date = new Date();
							/* var formattedDate = date.getFullYear()
									+ ("0" + (date.getMonth() + 1)).slice(-2)
									+ ("0" + date.getDate()).slice(-2)
									+ (date.getHours())
									+ ("0" + date.getMinutes()).slice(-2)
									+ ("0" + date.getSeconds()).slice(-2); */
							
							function pad2(n) { 
								return n < 10 ? '0' + n : n 
							}

							var formattedDate = date.getFullYear().toString() + pad2(date.getMonth() + 1) + pad2( date.getDate()) + pad2( date.getHours() ) + pad2( date.getMinutes() ) + pad2( date.getSeconds() );


							document.getElementById("statustimestamp").value = formattedDate;

							document.getElementById("selectedoption").value = this.options[this.selectedIndex].value;

							if (this.options[this.selectedIndex].value == "CHECKPAYMENTSTATUS"
									|| this.options[this.selectedIndex].value == "GETTRANSACTIONDETAILS"
									|| this.options[this.selectedIndex].value == "FETCHTRANSACTIONPERIOD") {

								transversionchange();

							} else {
								statusversionchange();
							}

							if (this.options[this.selectedIndex].value == 'PURCHASE') {
								$('#purchase').show();
								$('#refund').hide();
								$('#status').hide();
								$('#transtatus').hide();
								$('#errorMessage').hide();
								$('#checkboxes').show();
								$('#format').hide();
								$('#ver').hide();
								$('#refundver').hide();
								$('#refundinfodiv').hide();
								$('#addinfo').hide();
								$('#purchaseversion').show();
								$('#transver').hide();
								$('#netbanking').hide();
								$('#purchase_cc').hide();
								$('#si_card_debit').hide();
								//$('#SIAutoPay').hide();
								$('#BillPay').hide();
								$('#billpayversion').hide();
								$('#pg_collect_request_api').hide();
								$('#pgcollectrequestapiversion').hide();
								$('#offerverifymerchant').hide();
							}
							if (this.options[this.selectedIndex].value == 'PG_COLLECT_REQUEST_API') 
							{
								
								$('#purchase').hide();
								$('#refund').hide();
								$('#status').hide();
								$('#transtatus').hide();
								$('#errorMessage').hide();
								$('#checkboxes').hide();
								$('#format').hide();
								$('#ver').hide();
								$('#refundver').hide();
								$('#refundinfodiv').hide();
								$('#addinfo').hide();
								$('#purchaseversion').hide();
								$('#transver').hide();
								$('#netbanking').hide();
								$('#purchase_cc').hide();
								$('#si_card_debit').hide();
								//$('#SIAutoPay').hide();
								$('#BillPay').hide();
								$('#billpayversion').hide();
								$('#pg_collect_request_api').show();
								$('#pgcollectrequestapiversion').show();
								$('#offerverifymerchant').hide();
							}
							if (this.options[this.selectedIndex].value == 'INITIATE_MERCHANT_VERIFY') 
							{
								
								$('#purchase').hide();
								$('#refund').hide();
								$('#status').hide();
								$('#transtatus').hide();
								$('#errorMessage').hide();
								$('#checkboxes').hide();
								$('#format').hide();
								$('#ver').hide();
								$('#refundver').hide();
								$('#refundinfodiv').hide();
								$('#addinfo').hide();
								$('#purchaseversion').hide();
								$('#transver').hide();
								$('#netbanking').hide();
								$('#purchase_cc').hide();
								$('#si_card_debit').hide();
								//$('#SIAutoPay').hide();
								$('#BillPay').hide();
								$('#billpayversion').hide();
								$('#pg_collect_request_api').hide();
								$('#pgcollectrequestapiversion').hide();
								$('#offerverifymerchant').show();
								
								var date=new Date();
								function pad2(n) { 
									return n < 10 ? '0' + n : n 
								}
								var formattedDate1 = date.getFullYear().toString() + pad2(date.getMonth() + 1) + pad2( date.getDate()) + pad2( date.getHours() ) + pad2( date.getMinutes() ) + pad2( date.getSeconds() );

								
								document.getElementById("offerverifytxnamt").value= "1.00";
								document.getElementById("offerverifytxntime").value= formattedDate1;
								document.getElementById("offerverifynetbankingid").value = "";
								document.getElementById("offerverifycardnumber").value= "";
								document.getElementById("offerverifyvpa").value="";
								document.getElementById("offerverifycustomerid").value="";
								document.getElementById("offerverifydevicetype").value="";
								document.getElementById("offerverifydeviceos").value="";
								
							}
							
							if (this.options[this.selectedIndex].value == 'BILLPAY') {
								$('#purchase').show();
								$('#refund').hide();
								$('#status').hide();
								$('#transtatus').hide();
								$('#errorMessage').hide();
								$('#checkboxes').show();
								$('#format').hide();
								$('#ver').hide();
								$('#refundver').hide();
								$('#refundinfodiv').hide();
								$('#addinfo').hide();
								$('#purchaseversion').hide();
								$('#transver').hide();
								$('#netbanking').hide();
								$('#purchase_cc').hide();
								$('#SIAutoPay').hide();
								$('#BillPay').show();
								$('#billpayversion').show();
								$('#pg_collect_request_api').hide();
								$('#pgcollectrequestapiversion').hide();
								$('#offerverifymerchant').hide();
							}
							

							//purchase_cc
							if (this.options[this.selectedIndex].value == 'PURCHASE_CC') {
								console.log("selected channel::>>",this.options[this.selectedIndex].value);
								$('#purchase').hide();
								$('#purchase_cc').show();
								$('#refund').hide();
								$('#status').hide();
								$('#transtatus').hide();
								$('#errorMessage').hide();
								$('#checkboxes').hide();
								$('#format').hide();
								$('#ver').hide();
								$('#refundver').hide();
								$('#refundinfodiv').hide();
								$('#addinfo').hide();
