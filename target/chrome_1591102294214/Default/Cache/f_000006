	function postStatusRequest() {
		if (document.getElementById("statusrequest").value == ''
				|| document.getElementById("requestId").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			var requestData = document.getElementById('statusrequest').value;
			var serviceurl = document.getElementById('statusserviceurl').value;
			var format = document.getElementById("mode");
			var mode = format.options[format.selectedIndex].value;
			var requestId = document.getElementById("requestId").value;

			var e = document.getElementById("txntype");
			var selectedOption = e.options[e.selectedIndex].value;
			var apiname = selectedOption;
			console.log("apiname :", apiname);
			if (selectedOption == "CHECKPAYMENTSTATUS"
					|| selectedOption == "GETTRANSACTIONDETAILS"
					|| selectedOption == "FETCHTRANSACTIONPERIOD") {

				var v = document.getElementById("transversion");
				var version = v.options[v.selectedIndex].value;

			} else {
				var v = document.getElementById("statusversion");
				var version = v.options[v.selectedIndex].value;
			}

			var url = "postStatusRequest";

			$
					.post(
							url,
							{
								requestData : requestData,
								serviceurl : serviceurl,
								mode : mode,
								requestId : requestId,
								version : version,
								apiname : apiname
							},
							function(data, status) {
								//document.getElementById('statusresponse').value = data;
								var response = data.split("^");

								document.getElementById('statusresponse').value = response[0];
								document.getElementById('status.REQTIMES').value = response[1];
								document.getElementById('status.RESTIMES').value = response[2];
								document.getElementById('status.DIFFTIMES').value = response[3];

							})/* .done(function() {
																																																																																																																																																															    alert( "second success" );
																																																																																																																																																															  }); */
		}
	}

	function postTransRequest() {

		if (document.getElementById("transrequest").value == ''
				|| document.getElementById("transChecksum").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			var requestData = document.getElementById('transrequest').value;
			var serviceurl = document.getElementById('transserviceurl').value;
			var format = document.getElementById("mode");
			var mode = format.options[format.selectedIndex].value;
			var v = document.getElementById("transversion");
			var version = v.options[v.selectedIndex].value;
			var transId = document.getElementById("transTransactionId").value;
			var statusId = document.getElementById("statusId").value;
			/* var obj = JSON.parse(config);
			var url = obj[0].url; */
			$
					.ajax({
						url : 'postTransRequest',
						data : {
							requestData : requestData,
							serviceurl : serviceurl,
							mode : mode,
							transId : transId,
							statusId : statusId,
							version : version
						},
						success : function(responseText) {

							var response = responseText.split("^");

							document.getElementById('transresponse').value = response[0];
							document.getElementById('trans.REQTIMES').value = response[1];
							document.getElementById('trans.RESTIMES').value = response[2];
							document.getElementById('trans.DIFFTIMES').value = response[3];

						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								//alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});
		}
	}
	function postRefundRequest() {

		if (document.getElementById("refundrequest").value == ''
				|| document.getElementById("refundtxnref").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			var requestData = document.getElementById('refundrequest').value;
			var serviceurl = document.getElementById('refundserviceurl').value;
			var transref = document.getElementById("refundtxnref").value;
			var checksumseed = document.getElementById("refundchecksumseed").value;
			var format = document.getElementById("mode");
			var mode = format.options[format.selectedIndex].value;

			var v = document.getElementById("refundversion");
			var version = v.options[v.selectedIndex].value;

			$
					.ajax({
						url : 'postRefundRequest',
						data : {
							requestData : requestData,
							serviceurl : serviceurl,
							transref : transref,
							checksumseed : checksumseed,
							mode : mode,
							version : version

						},
						success : function(responseText) {

							var response = responseText.split("^");
							document.getElementById('refundresponse').value = response[0];
							document.getElementById('reschecksum').value = response[1];

							document.getElementById('refund.REQTIMES').value = response[3];
							document.getElementById('refund.RESTIMES').value = response[4];
							document.getElementById('refund.DIFFTIMES').value = response[5];

							if (response[2] == 'true') {
								$('#errorchecksum').hide();
								$('#successchecksum').show();
							} else {
								$('#errorchecksum').show();
								$('#successchecksum').hide();
							}

						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								//alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});
		}
	}

	function preparerequest() {
		$('#splittransaction').hide();
		var version = document.getElementById("version").value;

		if (document.getElementById("checksumseed").value == ''
				|| document.getElementById("clientId").value == ''
				|| document.getElementById("merchantId").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			$('#duplicate').hide();
			return false;

		} else {

			var randomBasket = "ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789";
			var randamKey = "";
			for (var i = 0; i < 12; i++) {
				randamKey += randomBasket.charAt(Math.floor(Math.random()
						* randomBasket.length));
			}

// 			var date = new Date();
// 			var formattedDate = date.getFullYear()
// 					+ ("0" + (date.getMonth() + 1)).slice(-2)
// 					+ ("0" + date.getDate()).slice(-2) + (date.getHours())
// 					+ ("0" + date.getMinutes()).slice(-2)
// 					+ ("0" + date.getSeconds()).slice(-2);

// 			document.getElementById("txnref").value = randamKey;
// 			document.getElementById("txntime").value = formattedDate;

			var env = document.getElementById("env").value;

			var e = document.getElementById("txntype");
			var selectedOption = e.options[e.selectedIndex].value;

			var si_flag = document.getElementById("si_flag").value;

			document.getElementById("selectedoption").value = selectedOption;

			document.getElementById("channel").value = document
					.getElementById("channel").options[document
					.getElementById("channel").selectedIndex].value;

			var clientId = document.getElementById("clientId").value;
			var merchantId = document.getElementById("merchantId").value;
			var merchantname = document.getElementById("merchantname").value;
			var checksumseed = document.getElementById("checksumseed").value;

			//var txnref = randamKey;
			var txncurrency = document.getElementById("txncurrency").value;
			//var txntime = formattedDate;
			var txnref = document.getElementById("txnref").value;//MH9754
			var txntime = document.getElementById("txntime").value;//MH9754

			var txnamt = document.getElementById("txnamt").value;
			var serviceurl = document.getElementById("serviceurl").value;
			var returnurl = document.getElementById("returnurl").value;
			var submerchantid = document.getElementById("submerchantid").value;
			var customername = document.getElementById("customername").value;
			var channel = document.getElementById("channel").value;
			var mobile = document.getElementById("mobile").value;
			var jiocustid = document.getElementById("jiocustid").value;
			var email = document.getElementById("email").value;
			var add1 = document.getElementById("add1").value;
			var add2 = document.getElementById("add2").value;
			var city = document.getElementById("city").value;
			var state = document.getElementById("state").value;
			var pincode = document.getElementById("pincode").value;
			var itemno0 = document.getElementById("itemno0").value;
			var itemno1 = document.getElementById("itemno1").value;
			var itemno2 = document.getElementById("itemno2").value;
			var itemno3 = document.getElementById("itemno3").value;
			var itemno4 = document.getElementById("itemno4").value;
			var itemno5 = document.getElementById("itemno5").value;
			var itemdesc0 = document.getElementById("itemdesc0").value;
			var itemdesc1 = document.getElementById("itemdesc1").value;
			var itemdesc2 = document.getElementById("itemdesc2").value;
			var itemdesc3 = document.getElementById("itemdesc3").value;
			var itemdesc4 = document.getElementById("itemdesc4").value;
			var itemdesc5 = document.getElementById("itemdesc5").value;
			var itemcost0 = document.getElementById("itemcost0").value;
			var itemcost1 = document.getElementById("itemcost1").value;
			var itemcost2 = document.getElementById("itemcost2").value;
			var itemcost3 = document.getElementById("itemcost3").value;
			var itemcost4 = document.getElementById("itemcost4").value;
			var itemcost5 = document.getElementById("itemcost5").value;
			var itemquantity0 = document.getElementById("itemquantity0").value;
			var itemquantity1 = document.getElementById("itemquantity1").value;
			var itemquantity2 = document.getElementById("itemquantity2").value;
			var itemquantity3 = document.getElementById("itemquantity3").value;
			var itemquantity4 = document.getElementById("itemquantity4").value;
			var itemquantity5 = document.getElementById("itemquantity5").value;

			//offerspurchase
			if(document.getElementById("applyoffercheckbox").checked==1)
			{
			var applyOfferCompulsorily = "Y";
			}
			if(document.getElementById("applyoffercheckbox").checked==0)
			{
			var applyOfferCompulsorily = "N";
			}
			var offerKey = document.getElementById("offerkey").value;
			var deviceType = document.getElementById("offerdevicetype").value;
			var deviceOs = document.getElementById("offerdeviceos").value;
			var offerreqid = document.getElementById("offerreqid").value;
			//offerspurchase
			
