<script>
	var counter = 0;

	function removecart(i) {
		$("#row" + i).hide();
		document.getElementById('itemno' + i).value = null;
		counter--;
		alert(counter);
		return false;
	}

	function generatesplit() {

		var splitdetail = new Array();

		for (var j = 0; j < 4; j++) {

			if (document.getElementById('mid' + j).value == ''
					|| document.getElementById('mid' + j).value == null) {

				splitdetail[j] = "";

				continue;
			} else {
				$("#splittrans").show();

				splitdetail[j] = "|" + document.getElementById('mid' + j).value
						+ "~" + document.getElementById('tid' + j).value + "~"
						+ document.getElementById('ref' + j).value + "~"
						+ document.getElementById('tamt' + j).value + "~"
						+ document.getElementById('agamt' + j).value + "~"
						+ document.getElementById('desc' + j).value + "~"
						+ document.getElementById('sudf1' + j).value + "~"
						+ document.getElementById('sudf2' + j).value + "~"
						+ document.getElementById('sudf3' + j).value + "~"
						+ document.getElementById('sudf4' + j).value + "~"
						+ document.getElementById('sudf5' + j).value;

			}

			if (splitdetail[2] == undefined) {
				splitdetail[2] = "";
			}
			if (splitdetail[3] == undefined) {
				splitdetail[3] = "";
			}
			if (splitdetail[1] == undefined) {
				splitdetail[1] = "";
			}
			if (splitdetail[0] == undefined) {
				splitdetail[0] = "";
			}

			var split = j + 1 + splitdetail[0] + splitdetail[1]
					+ splitdetail[2] + splitdetail[3];
		}

		document.getElementById('splitinput').value = split.trim();
		return false;
	}

	function removesplit(i) {
		$("#split" + i).hide();
		document.getElementById('mid' + i).value = null;

		counter--;

		if (counter == 0) {
			$("#splittrans").hide();
			$("#splitable").hide();

		}
		return false;
	}

	function removelocal(i) {
		$("#local" + i).hide();
		document.getElementById('localno' + i).value = null;
		counter--;
		return false;
	}

	function envdetails() {
		console.log("envdetails start")
		var en = document.getElementById("env");
		var env = en.options[en.selectedIndex].value;

		var tx = document.getElementById("txntype");
		var trans = tx.options[tx.selectedIndex].value;

		var paymentmode = document.getElementById("paymentmode").options[document
				.getElementById("paymentmode").selectedIndex].value;
		$
				.ajax({

					url : 'geturl',
					data : {
						env : env
					},
					type : 'POST',
					success : function(data) {

						var obj = JSON.parse(data);
						console.log("envdetails " + data);
						document.getElementById("serviceurl").value = obj[0].purchase_service_url;
						document.getElementById("refundserviceurl").value = obj[0].refund_service_url;
						document.getElementById("statusserviceurl").value = obj[0].status_service_url;
						document.getElementById("transserviceurl").value = obj[0].trans_service_url;
						document.getElementById("returnurl").value = obj[0].purchase_return_url;
						document.getElementById("refundreturnurl").value = obj[0].refund_return_url;
						document.getElementById("nb_serviceurl").value = obj[0].nb_purchase_service_url;
						document.getElementById("nb_return_url").value = obj[0].nb_purchase_return_url;

						document.getElementById("cc_serviceurl").value = obj[0].cc_serviceurl;
						document.getElementById("cc_return_url").value = obj[0].cc_return_url;

						document.getElementById("sicd_serviceurl").value = obj[0].si_serviceurl;
						document.getElementById("collect_service_url").value = obj[0].collect_service_url;
						document.getElementById("collectreturnurl").value = obj[0].collectreturnurl;

						if (trans == 'REFUND') {
							changeurl();
						}
						//UPI API
						if (trans == 'PG_COLLECT_REQUEST_API')
						{
						changecollecturl();
						}		
						if ((trans == 'PURCHASE') || (trans=='BILLPAY')) {
							if (pcicheckbox.checked == 1) {
								if (paymentmode == 'DEBITCARD'
										|| paymentmode == 'CREDITCARD') { 
									document.getElementById("serviceurl").value =obj[0].pcicard_service_url;
								}
								if (paymentmode == 'NETBANKING') {
									document.getElementById("serviceurl").value = obj[0].pcinet_service_url;
								}

							}
							if (env === 'ZDR') {
								document.getElementById("serviceurl").value = obj[0].dr_purchase;
							}

						}

					},
					error : function(err) {

						console.log('error in ajax call');
					}
				});

	}

	function preLoad() {

		/* Load Merchant info before loading page */
		$('#splittransaction').hide();
		getMerchantInfo();

		var en = document.getElementById("env");
		var env = en.options[en.selectedIndex].value;

		var d = new Date().getTime();
		var uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g,
				function(c) {
					var r = (d + Math.random() * 16) % 16 | 0;
					d = Math.floor(d / 16);
					return (c == 'x' ? r : (r & 0x3 | 0x8)).toString(16);
				});

		document.getElementById("starttime").value = "NA";
		document.getElementById("endtime").value = "NA";

		var tx = document.getElementById("txntype");
		document.getElementById("selectedoption").value = tx.options[tx.selectedIndex].value;

		document.getElementById("purchasebutton").addEventListener("dblclick",
				function(e) {
					alert('Warning !!! Double click not allowed');
				});

		document.getElementById("purchasebutton").addEventListener("click",
				preparerequest);
		//NETBANKING
		document.getElementById("nb_purchasebutton").addEventListener(
				"dblclick", function(e) {
					alert('Warning !!! Double click not allowed');
				});

		document.getElementById("nb_purchasebutton").addEventListener("click",
				prepareNbPurchaseRequest);

