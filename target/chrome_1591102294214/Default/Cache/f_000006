			var localno1 = document.getElementById("localno1").value;
			var localno2 = document.getElementById("localno2").value;
			var localno3 = document.getElementById("localno3").value;

			var localdesc0 = document.getElementById("localdesc0").value;
			var localdesc1 = document.getElementById("localdesc1").value;
			var localdesc2 = document.getElementById("localdesc2").value;
			var localdesc3 = document.getElementById("localdesc3").value;

			var localoption0 = document.getElementById("localoption0").value;
			var localoption1 = document.getElementById("localoption1").value;
			var localoption2 = document.getElementById("localoption2").value;
			var localoption3 = document.getElementById("localoption3").value;

			var expmon = document.getElementById("cardinfo.expmon").value;
			var expyr = document.getElementById("cardinfo.expyr").value;
			var nickname = document.getElementById("cardinfo.nickname").value;
			var cardtype = document.getElementById("cardinfo.cardtype").value;
			var netcardtype = document.getElementById("netbanking.cardtype").value;
			var walletcardtype = document.getElementById("wallet.cardtype").value;

			var pan = document.getElementById("cardinfo.pan").value;
			var cvv2 = document.getElementById("cardinfo.cvv2").value;

			var globalnote = document.getElementById("merchantData.globalnote").value;
			var udf1 = document.getElementById("udf1").value;
			var udf2 = document.getElementById("udf2").value;
			var udf3 = document.getElementById("udf3").value;
			var udf4 = document.getElementById("udf4").value;
			var udf5 = document.getElementById("udf5").value;
			var prodDesc = document.getElementById("prodDesc").value;

			var bankid = document.getElementById("cardinfo.bankid").value;
			var ipaddress = document.getElementById("client.ipaddress").value;
			var macaddress = document.getElementById("client.macaddress").value;
			var deviceid = document.getElementById("client.deviceid").value;
			var splitinput = document.getElementById("splitinput").value;
			var netnickname = document.getElementById("netbanking.nickname").value;

			var netbankid = document.getElementById("netbanking.bankid").options[document
					.getElementById("netbanking.bankid").selectedIndex].value;

			var walletbankid = document.getElementById("wallet.name").options[document
					.getElementById("wallet.name").selectedIndex].value;

			var paymentmode = document.getElementById("paymentmode").options[document
					.getElementById("paymentmode").selectedIndex].value;

			var dsbaccno = document.getElementById("dsbaccountnumber").value;
			var consent = document.getElementById("consentflag").value;

			var include = document
					.getElementById("merchantData.preferences.includepaymentmodes");

			var include1 = [];
			for (var i = 0; i < include.length; i++) {
				if (include.options[i].selected) {
					include1.push(include.options[i].value);
				}
			}

			var includemode = include1.join("|");

			var exclude = document
					.getElementById("merchantData.preferences.excludepaymentmodes");

			var exclude1 = [];
			for (var i = 0; i < exclude.length; i++) {
				if (exclude.options[i].selected) {
					exclude1.push(exclude.options[i].value);
				}
			}

			var excludemode = exclude1.join("|");

			var d = document
					.getElementById("merchantData.preferences.defaultpaymentmodes");
			var declude = d.options[d.selectedIndex].value;

			/* 		var declude1 = [];
					for (var i = 0; i < declude.length; i++) {
						if (declude.options[i].selected) {

							declude1.push(declude.options[i].value);

						}
					} */

			//	var decludemode = declude1.join("|");
			document.getElementById('include').value = includemode;
			document.getElementById('exclude').value = excludemode;
			document.getElementById('declude').value = declude;
			
			
			//SI Auto Pay 23/08/2018
			var SIProductID = document.getElementById("SIProductID").value;
			var SIProductDesc = document.getElementById("SIProductDesc").value;
			var SIExpDate = document.getElementById("SIExpDate").value;
			var SIMaxAmount = document.getElementById("SIMaxAmount").value;
			var SIFrequency = document.getElementById("SIFrequency").value;
			var SIAmtType = document.getElementById("SIAmtType").value;
			var SICustID = document.getElementById("SICustID").value;
			
			
			//BILLPAY 23/08/2018
			var bversion = document.getElementById("bversion").value;
			var BillID = document.getElementById("BillID").value;
			var BillerID = document.getElementById("BillerID").value;
			var BillerName = document.getElementById("BillerName").value;
			var Authenticator1 = document.getElementById("Authenticator1").value;
			var Authenticator2 = document.getElementById("Authenticator2").value;
			var Authenticator3 = document.getElementById("Authenticator3").value;
			var Authenticator4 = document.getElementById("Authenticator4").value;
			var Authenticator5 = document.getElementById("Authenticator5").value;
			var Authenticator6 = document.getElementById("Authenticator6").value;
			var Authenticator7 = document.getElementById("Authenticator7").value;
			var PayType = document.getElementById("PayType").value;
			var BankMsg = document.getElementById("BankMsg").value;
			var AdditionalTags = document.getElementById("AdditionalTags").value;
			var BillDetails = document.getElementById("BillDetails").value;
			
			var jmAccTypeFlagIndex = document.getElementById("jmAccTypeFlag");
			var jmAccTypeFlag = jmAccTypeFlagIndex.options[jmAccTypeFlagIndex.selectedIndex].value;
			var jmAccTypeIndex = document.getElementById("jmAccType");
			var jmAccType = jmAccTypeIndex.options[jmAccTypeIndex.selectedIndex].value;
			

			$
					.ajax({
						url : 'prepareMerchantReq',
						data : {
							applyOfferCompulsorily : applyOfferCompulsorily,
							offerkey : offerKey,
							deviceType : deviceType,
							deviceOS : deviceOs,
							offerreqid : offerreqid,
							clientId : clientId,
							merchantId : merchantId,
							checksumseed : checksumseed,
							txnref : txnref,
							txncurrency : txncurrency,
							txnamt : txnamt,
							selectedOption : selectedOption,
							txntime : txntime,
							customername : customername,
							channel : channel,
							environment : env,
							merchantname : merchantname,
							serviceurl : serviceurl,
							returnurl : returnurl,
							submerchantid : submerchantid,
							add1 : add1,
							add2 : add2,
							state : state,
							city : city,
							pincode : pincode,
							mobile : mobile,
							email : email,
							jiocustid : jiocustid,
							itemno0 : itemno0,
							itemdesc0 : itemdesc0,
							itemcost0 : itemcost0,
							itemquantity0 : itemquantity0,
							itemno1 : itemno1,
							itemdesc1 : itemdesc1,
							itemcost1 : itemcost1,
							itemquantity1 : itemquantity1,
							itemno2 : itemno2,
							itemdesc2 : itemdesc2,
							itemcost2 : itemcost2,
							itemquantity2 : itemquantity2,
							itemno3 : itemno3,
							itemdesc3 : itemdesc3,
							itemcost3 : itemcost3,
							itemquantity3 : itemquantity3,
							itemno4 : itemno4,
							itemdesc4 : itemdesc4,
							itemcost4 : itemcost4,
							itemquantity4 : itemquantity4,
							globalnote : globalnote,
							udf1 : udf1,
							udf2 : udf2,
							udf3 : udf3,
							udf4 : udf4,
							udf5 : udf5,
							localno0 : localno0,
							localdesc0 : localdesc0,
							localoption0 : localoption0,
							localno1 : localno1,
							localdesc1 : localdesc1,
							localoption1 : localoption1,
							localno2 : localno2,
							localdesc2 : localdesc2,
							localoption2 : localoption2,
							localno3 : localno3,
							localdesc3 : localdesc3,
							localoption3 : localoption3,
							nickname : nickname,
							expmon : expmon,
							expyr : expyr,
							cardtype : cardtype,
							cvv2 : cvv2,
							pan : pan,
							bankid : bankid,
							deviceid : deviceid,
							macaddress : macaddress,
							ipaddress : ipaddress,
							splitinput : splitinput,
							netnickname : netnickname,
							netbankid : netbankid,
							paymentmode : paymentmode,
							netcardtype : netcardtype,
							walletbankid : walletbankid,
							walletcardtype : walletcardtype,
							includemode : includemode,
							excludemode : excludemode,
							decludemode : declude,
							version : version,
							si_flag : si_flag,
							prodDesc : prodDesc,
							dsbaccno : dsbaccno,
							consent : consent,
							
							SIProductID:SIProductID,
							SIProductDesc:SIProductDesc,
							SIExpDate:SIExpDate,
							SIMaxAmount:SIMaxAmount,
							SIFrequency:SIFrequency,
							SIAmtType:SIAmtType,
							SICustID:SICustID,
							
							bversion:bversion,
							BillID:BillID,
							BillerID:BillerID,
							BillerName:BillerName,
							Authenticator1:Authenticator1,
							Authenticator2:Authenticator2,
							Authenticator3:Authenticator3,
							Authenticator4:Authenticator4,
							Authenticator5:Authenticator5,
							Authenticator6:Authenticator6,
							Authenticator7:Authenticator7,
							PayType:PayType,
							BankMsg:BankMsg,
							AdditionalTags:AdditionalTags,
							BillDetails:BillDetails,
							jmAccTypeFlag: jmAccTypeFlag,
							jmAccType:jmAccType
						},
						success : function(responseText) {
							var response = responseText.split("^");

							if (responseText == 'NOT-AUTHORIZED') {

								document.myform.action = "Error.jsp";
								document.myform.submit();

							} else {

								$('#errorMessage').hide();
								document.getElementById('checksum').value = response[0];
								document.getElementById('requestinfo').value = response[1];
								document.getElementById('reqinfo').value = response[2];

								if (response[3] == 'TRUE') {

									$('#duplicate').show();

								} else {

									$('#duplicate').hide();
								}

							}

						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								//alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});
		}
	}

	var merchantData;
	function onMerchantInfoNameChanged() {

		var e = document.getElementById("merchantInfoName");
		var selectedOption = e.options[e.selectedIndex].value;
		console.log("merchantInfoName selected value :" + selectedOption);

		/* to get index of merchantInfo */
		var merchant;
		for (var i = 0; i < merchantData.length; i++) {
			if (merchantData[i].merchantName === selectedOption) {
				merchant = merchantData[i];
				console.log(" index :" + selectedOption);

				setMerchantInfo();
			}
		}

		if (merchant != undefined) {
			
			document.getElementById("collectmerchantId").value = merchant.merchantId;
			document.getElementById("merchantId").value = merchant.merchantId;
			document.getElementById("transmerchantid").value = merchant.merchantId;
			document.getElementById("refundmerchantId").value = merchant.merchantId;
			document.getElementById("statusMerchantId").value = merchant.merchantId;
			document.getElementById("offerverifymerchantId").value=merchant.merchantId;
			
			document.getElementById("clientId").value = merchant.clientId;
			document.getElementById("refundclientId").value = merchant.clientId;
			document.getElementById("transclientid").value = merchant.clientId;
			document.getElementById("collectclientId").value=merchant.clientId;

			document.getElementById("checksumseed").value = merchant.checksumKey;
			document.getElementById("refundchecksumseed").value = merchant.checksumKey;
			document.getElementById("transChecksumseed").value = merchant.checksumKey;
			document.getElementById("statusChecksumseed").value = merchant.checksumKey;
			document.getElementById("collectchecksumseed").value = merchant.checksumKey;
			
			document.getElementById("collectmerchantname").value = selectedOption;
			document.getElementById("offerverifymerchantname").value=selectedOption;
			
			//NETBANKING
			document.getElementById("nb_merchant_id").value = merchant.merchantId;
			document.getElementById("nb_client_id").value = merchant.clientId;
			document.getElementById("nb_checksumseed").value = merchant.checksumKey;

		} else if (selectedOption === "default_selection") {
			document.getElementById("merchantId").value = "";
			document.getElementById("transmerchantid").value = "";
			document.getElementById("refundmerchantId").value = "";
			document.getElementById("statusMerchantId").value = "";
			document.getElementById("collectmerchantId").value = "";
			
			
			document.getElementById("clientId").value = "";
			document.getElementById("refundclientId").value = "";
			document.getElementById("transclientid").value = "";
			document.getElementById("collectclientId").value="";

			document.getElementById("checksumseed").value = "";
			document.getElementById("refundchecksumseed").value = "";
			document.getElementById("transChecksumseed").value = "";
			document.getElementById("statusChecksumseed").value = "";
			document.getElementById("collectchecksumseed").value = "";

			//NETBANKING
			document.getElementById("nb_merchant_id").value = "";
			document.getElementById("nb_client_id").value = "";
			document.getElementById("nb_checksumseed").value = "";
		}
		

	}

	function getMerchantInfo() {

		$.ajax({

			url : 'getMerchantInfo',
			type : 'GET',
			success : function(data) {
				var select = document.getElementById("merchantInfoName");
				console.log("select :", select);
				merchantData = JSON.parse(data);
				for (var int = 0; int < merchantData.length; int++) {
					console.log("merchantName :",
							merchantData[int].merchantName)
					var option = document.createElement("option");
					option.text = merchantData[int].merchantName;
					option.value = merchantData[int].merchantName;
					select.appendChild(option);
				}
			},
			error : function(err) {

				console.log('error in ajax call');
			}
		});
	}

	function pciToogle() {
		if (pcicheckbox.checked == 1) {
			$('#pci').show();
			$('#pciextra').show();

		} else {
			envdetails();
			$('#pci').hide();
			$('#pciextra').hide();
		}
	}

	function offerToogle(){
		if(offercheckbox.checked==1){
			$('#offer').show();
		}
		else{
			$('#offer').hide();
		}
	}
	function splitToogle() {
		if (splitcheckbox.checked == 1) {
			$('#split').show();

		} else {

			$('#split').hide();
		}
	}
	
// 	function onChangePaymentMode() {
// 		envdetails();
// 	}

	function prepareStatusRequest() {

		if (document.getElementById("statusChecksumseed").value == ''
				|| document.getElementById("statusMerchantId").value == ''
				|| document.getElementById("statusTransactionId").value == ''
				|| document.getElementById("requestId").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			var env = document.getElementById("env").value.trim();

			var e = document.getElementById("txntype");
			var selectedOption = e.options[e.selectedIndex].value;
			document.getElementById("selectedoption").value = selectedOption;

			var format = document.getElementById("mode");
			var mode = format.options[format.selectedIndex].value;

			if (selectedOption == "CHECKPAYMENTSTATUS"
					|| selectedOption == "GETTRANSACTIONDETAILS"
					|| selectedOption == "FETCHTRANSACTIONPERIOD") {

				var v = document.getElementById("transversion");
				var version = v.options[v.selectedIndex].value;

			} else {
				var v = document.getElementById("statusversion");
				var version = v.options[v.selectedIndex].value;
			}

			var merchantId = document.getElementById("statusMerchantId").value;
			var checksumseed = document.getElementById("statusChecksumseed").value;

			var serviceurl = document.getElementById("statusserviceurl").value;

			var transId = document.getElementById("statusTransactionId").value;

			var starttime = document.getElementById("starttime").value;

			var endtime = document.getElementById("endtime").value;

			var requestId = document.getElementById("requestId").value;

			var timestamp = document.getElementById("statustimestamp").value;

			var txntimestamp = document.getElementById("statustxntimestamp").value;

			$
					.ajax({
						url : 'prepareMerchantReq',
						data : {
							mode : mode,
							requestId : requestId,
							starttime : starttime,
							endtime : endtime,
							environment : env,
							merchantId : merchantId,
							serviceurl : serviceurl,
							transactionId : transId,
							checksumseed : checksumseed,
							selectedOption : selectedOption,
							version : version,
							txntime : timestamp,
							jiotxntime : txntimestamp
						},
						success : function(responseText) {

							var response = responseText.split("^");

							if (responseText == 'NOT-AUTHORIZED') {

								document.myform.action = "Error.jsp";
								document.myform.submit();

							} else {

								document.getElementById('statusChecksum').value = response[0];
								document.getElementById('statusrequest').value = response[1];
								document.getElementById('statusReqinfo').value = response[2];

								$('#errorMessage').hide();
								$('#message').show();
							}

						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								//alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});

		}
	}

	function prepareTransRequest() {

		if (document.getElementById("transChecksumseed").value == ''
				|| document.getElementById("transmerchantid").value == ''
				|| document.getElementById("transclientid").value == ''
				|| document.getElementById("transTransactionId").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			var env = document.getElementById("env").value.trim();

			var e = document.getElementById("txntype");
			var selectedOption = e.options[e.selectedIndex].value;
			document.getElementById("selectedoption").value = selectedOption;

			var format = document.getElementById("mode");
			var mode = format.options[format.selectedIndex].value;

			var merchantId = document.getElementById("transmerchantid").value;

			var clientId = document.getElementById("transclientid").value;
			var checksumseed = document.getElementById("transChecksumseed").value;
			var serviceurl = document.getElementById("transserviceurl").value;

			var transId = document.getElementById("transTransactionId").value;

			var v = document.getElementById("transversion");
			var version = v.options[v.selectedIndex].value;

			/* 	var obj = JSON.parse(config);
				var url = obj[0].url; */

			$
					.ajax({
						url : 'prepareMerchantReq',
						data : {
							mode : mode,
							clientId : clientId,
							environment : env,
							merchantId : merchantId,
							serviceurl : serviceurl,
							transactionId : transId,
							checksumseed : checksumseed,
							selectedOption : selectedOption,
							transactionId : transId,
							version : version
						},
						success : function(responseText) {

							var response = responseText.split("~");

							if (responseText == 'NOT-AUTHORIZED') {

								document.myform.action = "Error.jsp";
								document.myform.submit();

							} else {

								document.getElementById('transChecksum').value = response[0];
								document.getElementById('transrequest').value = response[1];
								document.getElementById('transReqinfo').value = response[2];
								document.getElementById('statusId').value = response[3];

								$('#errorMessage').hide();
								$('#message').show();
							}
						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								//alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});
		}
	}

	function prepareRefundRequest() {

		if (document.getElementById("refundchecksumseed").value == ''
				|| document.getElementById("refundclientId").value == ''
				|| document.getElementById("refundmerchantId").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			console.log("in preparerefundrequest")
			var env = document.getElementById("env").value.trim();

			var e = document.getElementById("txntype");
			var selectedOption = e.options[e.selectedIndex].value;

			var v = document.getElementById("refundversion");
			var version = v.options[v.selectedIndex].value;

			var format = document.getElementById("mode");
			var mode = format.options[format.selectedIndex].value;

			document.getElementById("selectedoption").value = selectedOption;

			document.getElementById("refundchannel").value = document
					.getElementById("refundchannel").options[document
					.getElementById("refundchannel").selectedIndex].value;

			var clientId = document.getElementById("refundclientId").value;
			var merchantId = document.getElementById("refundmerchantId").value;
			var merchantname = document.getElementById("refundmerchantname").value;
			var checksumseed = document.getElementById("refundchecksumseed").value;

			var txnref = document.getElementById("refundtxnref").value;
			var txncurrency = document.getElementById("refundtxncurrency").value;
			var txntime = document.getElementById("refundtxntime").value;

			var jiotxnref = document.getElementById("jiotxnref").value;

			var jiotxntime = document.getElementById("jiotxntime").value;

			var txnamt = document.getElementById("refundtxnamt").value;
			var serviceurl = document.getElementById("refundserviceurl").value;
			var returnurl = document.getElementById("refundreturnurl").value;
			var submerchantid = document.getElementById("refundsubmerchantid").value;
			var addinfo = document.getElementById("addinfm").value;

			var channel = document.getElementById("refundchannel").value;
			
			//UDF1 and UDF2 01/10/2018
			var udf1=document.getElementById("refUDF1").value;
			var udf2=document.getElementById("refUDF2").value;
			

			$
					.ajax({
						url : 'prepareMerchantReq',
						data : {
							clientId : clientId,
							merchantId : merchantId,
							checksumseed : checksumseed,
							txnref : txnref,
							txncurrency : txncurrency,
							txnamt : txnamt,
							selectedOption : selectedOption,
							txntime : txntime,
							channel : channel,
							environment : env,
							merchantname : merchantname,
							serviceurl : serviceurl,
							returnurl : returnurl,
							submerchantid : submerchantid,
							jiotxnref : jiotxnref,
							jiotxntime : jiotxntime,
							submerchantid : submerchantid,
							merchantname : merchantname,
							version : version,
							mode : mode,
							addinfo : addinfo,
							udf1:udf1,
							udf2:udf2

						},
						success : function(responseText) {

							var response = responseText.split("~");

							if (responseText == 'NOT-AUTHORIZED') {

								document.myform.action = "Error.jsp";
								document.myform.submit();

							} else {

								document.getElementById('refundchecksum').value = response[0];
								document.getElementById('refundrequest').value = response[1];
								document.getElementById('refundreqinfo').value = response[2];
								document.getElementById('refundinfo').value = response[3];

								$('#errorMessage').hide();
								$('#message').show();
							}

						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								//alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});
		}

	}
	
	//offers
	function prepareofferverifyRequest()
		{
			if(document.getElementById("offerverifymerchantId").value == '')
				{
				$('#errorMessage').show();
				$('message').hide();
				return false;
				}
			else{
				
				
				var env = document.getElementById("env").value.trim();
				var e = document.getElementById("txntype");
				var selectedOption = e.options[e.selectedIndex].value;
				var merchantId = document.getElementById("offerverifymerchantId").value;
				var txntime = document.getElementById("offerverifytxntime").value;
				var customerid = document.getElementById("offerverifycustomerid").value;
				var txnamt = document.getElementById("offerverifytxnamt").value;
				var serviceurl = document.getElementById("offerverify_service_url").value;
				//var returnurl = document.getElementById("offerverifyreturnurl").value;
				var vpa = document.getElementById("offerverifyvpa").value;
				var netbankingid = document.getElementById("offerverifynetbankingid").value;
				var paymentmode = document.getElementById("offerpaymentmode").value;
				var offerkey = document.getElementById("offerverifykey").value;
				var offerreqid = document.getElementById("offerverifyreqid").value;
				var devicetype = document.getElementById("offerverifydevicetype").value;
				var deviceos = document.getElementById("offerverifydeviceos").value;
				var cardnumber = document.getElementById("offerverifycardnumber").value;
				var apiName = document.getElementById("offerverifyapiname").value;
				var merchantName = document.getElementById("offerverifymerchantname").value;
				var RRN = document.getElementById("offerverifyrrn").value;
				$
				.ajax({
					
					url : 'prepareMerchantReq',
					data : {
						merchantId : merchantId,
						txnamt : txnamt,
						selectedOption : selectedOption,
						txntime : txntime,
						environment : env,
						merchantname : merchantName,
						serviceurl : serviceurl,
						//returnurl : returnurl,
						txnamt : txnamt,
						offerreqid : offerreqid,
						offerkey : offerkey,
						deviceType : devicetype,
						deviceOS : deviceos,
						customerid : customerid,
						cardNumber : cardnumber,
						netbankingid : netbankingid,
						paymentmode : paymentmode,
						vpa : vpa,
						apiName : apiName,
						RRN : RRN
						
					
				},
				success : function(responseText) {

					var response = responseText.split("~");

					if (responseText == 'NOT-AUTHORIZED') {

						document.myform.action = "Error.jsp";
						document.myform.submit();

					} else {

						document.getElementById('offerverifyrequest').value = response[0];

						$('#errorMessage').hide();
						$('#message').show();
					}

				},
				error : function(jqXHR, textStatus, errorThrown) {
					if (jqXHR.status == 500) {
						//alert('Internal error: ' + jqXHR.responseText);
					} else {
						//alert('Unexpected error.');
					}
				}
			});	
						
			}
		}
	
	
	//UPI API
	

	function prepareCollectRequest()
	{
		if (document.getElementById("collectchecksumseed").value == ''
			|| document.getElementById("collectclientId").value == ''
			|| document.getElementById("collectmerchantId").value == '') 
		{

		$('#errorMessage').show();
		$('#message').hide();
		
		console.log("inside error");
		return false;
		}

	else {

		console.log("inside preparecollectrequest");
		var env = document.getElementById("env").value.trim();

		var e = document.getElementById("txntype");
		
		var selectedOption = e.options[e.selectedIndex].value;
		
		var type = document.getElementById("collecttype").value;
		console.log(type);
		
		var v = document.getElementById("collectversion");
		var version = v.options[v.selectedIndex].value;

		
		
		
		document.getElementById("collectchannel").value = document
				.getElementById("collectchannel").options[document
				.getElementById("collectchannel").selectedIndex].value;
		
		var clientId = document.getElementById("collectclientId").value;
		var merchantId = document.getElementById("collectmerchantId").value;
		var checksumseed = document.getElementById("collectchecksumseed").value;
		var merchantname = document.getElementById("collectmerchantname").value;
		var collectcustomerid = document.getElementById("collectcustid").value;

		var txnref = document.getElementById("collecttxnref").value;
		var txncurrency = document.getElementById("collecttxncurrency").value;
		var txntime = document.getElementById("collecttxntime").value;
		var txnamt = document.getElementById("collecttxnamt").value;
		var serviceurl = document.getElementById("collect_service_url").value;
		var returnurl = document.getElementById("collectreturnurl").value;
		var channel = document.getElementById("collectchannel").value;
		
		var udf1=document.getElementById("collectUDF1").value;
		var udf2=document.getElementById("collectUDF2").value;
		var udf3=document.getElementById("collectUDF3").value;
		var udf4=document.getElementById("collectUDF4").value;
		var udf5=document.getElementById("collectUDF5").value;
		var proddesc=document.getElementById("collectproddesc").value;
		
		var upiCustomerVpa=document.getElementById("collectvpa").value;
		var upiTransactionExpiry=document.getElementById("collectexpiry").value;	
		var collectDeviceInfo=document.getElementById("collectdeviceinfo").value;
		var fundingsource=document.getElementById("collectfundingsource").value;
		var mobileNumber=document.getElementById("collectmobile").value;
		var customercity=document.getElementById("customercity").value;
		var customername=document.getElementById("collectcustomername").value;
		var customerstate=document.getElementById("customerstate").value;
		$
				.ajax({
					url : 'prepareMerchantReq',
					data : {
						clientId : clientId,
						merchantId : merchantId,
						checksumseed : checksumseed,
						txnref : txnref,
						txncurrency : txncurrency,
						txnamt : txnamt,
						selectedOption : selectedOption,
						txntime : txntime,
						channel : channel,
						environment : env,
						merchantname : merchantname,
						serviceurl : serviceurl,
						returnurl : returnurl,
						merchantname : merchantname,
						version : version,
						udf1:udf1,
						udf2:udf2,
						udf3:udf3,
						udf4:udf4,
						udf5:udf5,
						upiCustomerVpa:upiCustomerVpa,
						upiTransactionExpiry:upiTransactionExpiry,
						collecttype:type,
						deviceInfo:collectDeviceInfo,
						fundingSource:fundingsource,
						collectmobile:mobileNumber,
						prodDesc:proddesc,
						collectcustid:collectcustomerid,
						customercity:customercity,
						customerstate:customerstate,
						customername:customername
					},
					success : function(responseText) {

						var response = responseText.split("~");

						if (responseText == 'NOT-AUTHORIZED') {

							document.myform.action = "Error.jsp";
							document.myform.submit();

						} else {

							document.getElementById('collectchecksum').value = response[0];
							document.getElementById('collectrequest').value = response[1];
							document.getElementById('collectreqinfo').value = response[2];
							document.getElementById('collectinfo').value = response[3];

							$('#errorMessage').hide();
							$('#message').show();
						}

					},
					error : function(jqXHR, textStatus, errorThrown) {
						if (jqXHR.status == 500) {
							//alert('Internal error: ' + jqXHR.responseText);
						} else {
							//alert('Unexpected error.');
						}
					}
				});	
	}	
	}
	//NETBANKING
	function prepareNbPurchaseRequest() {

		if (document.getElementById("nb_checksumseed").value == ''
				|| document.getElementById("nb_client_id").value == ''
				|| document.getElementById("nb_merchant_id").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			var env = document.getElementById("env").value.trim();

			var e = document.getElementById("txntype");
			var selectedOption = e.options[e.selectedIndex].value;

			document.getElementById("selectedoption").value = selectedOption;

			var checksumseed = document.getElementById("nb_checksumseed").value;
			var apiname = document.getElementById("nb_api_name").value;
			var version = document.getElementById("nb_api_version").value;
			var merchantId = document.getElementById("nb_merchant_id").value;
			var txnref = document.getElementById("nb_merchant_txn_ref_no").value;
			var txnamt = document.getElementById("nb_txn_amount").value;
			var customerId = document.getElementById("nb_customer_id").value;
			var custMobile = document.getElementById("nb_customer_mobile_no").value;
			var clientId = document.getElementById("nb_client_id").value;
			var returnurl = document.getElementById("nb_return_url").value;
			var txntime = document.getElementById("nb_timestamp").value;
			var transactionType = document.getElementById("nb_txn_type").value;
			var txncurrency = document.getElementById("nb_txn_currency").value;
			var customername = document.getElementById("nb_customer_name").value;
			var custEmailId = document.getElementById("nb_customer_email").value;
			var custAdd1 = document.getElementById("nb_customer_addr_line1").value;
			var custAdd2 = document.getElementById("nb_customer_addr_line2").value;
			var custCity = document.getElementById("nb_customer_city").value;
			var custState = document.getElementById("nb_customer_state").value;
			var custZipcode = document.getElementById("nb_customer_zipcode").value;
			var productDesc = document.getElementById("nb_product_description").value;
			var udf1 = document.getElementById("nb_udf1").value;
			var udf2 = document.getElementById("nb_udf2").value;
			var udf3 = document.getElementById("nb_udf3").value;
			var udf4 = document.getElementById("nb_udf4").value;
			var udf5 = document.getElementById("nb_udf5").value;
			var ipAddress = document.getElementById("nb_ip_address").value;
			var userAgent = document.getElementById("nb_useragent").value;
			var cardNumber = document.getElementById("nb_card_number").value;
			var cardName = document.getElementById("nb_card_name").value;
			var cardCVV = document.getElementById("nb_card_cvv").value;
			var cardExpMonth = document.getElementById("nb_card_expiry_month").value;
			var CardExpYear = document.getElementById("nb_card_expiry_year").value;
			var fundingSource = document.getElementById("nb_funding_source").value;
			var bankId = document.getElementById("nb_bank_id").options[document.getElementById("nb_bank_id").selectedIndex].value;
			//var bankName = document.getElementById("nb_bank_id").options[document.getElementById("nb_bank_id").selectedIndex].text;
			var eeee = document.getElementById("nb_bank_id");
			var bankName = eeee.options[eeee.selectedIndex].text;
			var cardNickname = document.getElementById("nb_card_nickname").value;
			var cardTokenId = document.getElementById("nb_card_token_id").value;
			var siFlag = document.getElementById("nb_si_flag").value;
			var siCustId = document.getElementById("nb_si_cusomterid").value;
			var siMobileNo = document.getElementById("nb_si_mobilenumber").value;
			var siAmount = document.getElementById("nb_si_amount").value;
			var siFrequency = document.getElementById("nb_si_frequency").value;
			var jmAccountType = document.getElementById("nb_jm_account_type").value;
			var serviceurl = document.getElementById("nb_serviceurl").value;
			var channel = document.getElementById("nb_channel").value;

			$
					.ajax({
						url : 'prepareMerchantReq',
						data : {
							clientId : clientId,
							merchantId : merchantId,
							checksumseed : checksumseed,
							txnref : txnref,
							txncurrency : txncurrency,
							txnamt : txnamt,
							selectedOption : selectedOption,
							txntime : txntime,
							apiname : apiname,
							environment : env,
							customerId : customerId,
							serviceurl : serviceurl,
							returnurl : returnurl,
							mobile : custMobile,
							email : custEmailId,
							add1 : custAdd1,
							version : version,
							customername : customername,
							transactionTypeNB : transactionType,
							add2 : custAdd2,
							city : custCity,
							state : custState,
							pincode : custZipcode,
							prodDesc : productDesc,
							udf1 : udf1,
							udf2 : udf2,
							udf3 : udf3,
							udf4 : udf4,
							udf5 : udf5,
							ipaddress : ipAddress,
							userAgent : userAgent,
							cardNumber : cardNumber,
							cardName : cardName,
							cvv2 : cardCVV,
							expmon : cardExpMonth,
							expyr : CardExpYear,
							fundingSource : fundingSource,
							bankid : bankId,
							bankName : bankName,
							nickname : cardNickname,
							cardtoken : cardTokenId,
							si_flag : siFlag,
							si_customerId : siCustId,
							si_mobileNo : siMobileNo,
							si_amount : siAmount,
							si_frequency : siFrequency,
							jmAccountType : jmAccountType,
							channel : channel
						},
						success : function(responseText) {

							var response = responseText.split("~");
							console.log("response is: " + responseText);

							if (responseText == 'NOT-AUTHORIZED') {

								document.myform.action = "Error.jsp";
								document.myform.submit();

							} else {

								document.getElementById("nb_Checksum").value = response[0];
								document.getElementById('nb_request').value = response[1];
								document.getElementById('nb_checksum_info').value = response[2];

								$('#errorMessage').hide();
								$('#message').show();
							}

						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});
		}

	}

	function isJson(str) {
		try {
			JSON.parse(str);
		} catch (e) {
			return false;
		}
		return true;
	}

	function parseNbEnrollResponse() {
		var resp = document.getElementById("nb_response").value;

		if (isJson(resp)) {
			var obj = JSON.parse(resp);

			if (obj.payload_data.error_code == '000') {
				//document.getElementById("nb_bankposturl").value = obj.payload_data.acs_url;
				document.getElementById("nb_bankposturl").value = obj.payload_data.bank_redirect_url;
				var jsonObj = obj.payload_data.post_to_bank;
				var html = '';
				var type = 'hidden';
				$
						.each(
								jsonObj,
								function(key, value) {
									html += '<input type="'+type+'" name="'+ key +'" value="'+ value +'" />';
								});
				html += '<input type="' + type + '" name="return_url" value="'
						+ document.getElementById("nb_return_url").value
						+ '" />';
				//console.log(html);			
				document.getElementById("nb_requestinfo").value = html;
			}
		}
	}

	function prepareNbPostRequest() {

		if (document.getElementById("nb_response").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			var env = document.getElementById("env").value.trim();

			var e = document.getElementById("txntype");
			var selectedOption = e.options[e.selectedIndex].value;

			document.getElementById("selectedoption").value = selectedOption;

			var checksumseed = document.getElementById("nb_checksumseed").value;
			var apiname = document.getElementById("nb_api_name").value;
			var version = document.getElementById("nb_api_version").value;
			var merchantId = document.getElementById("nb_merchant_id").value;
			var txnref = document.getElementById("nb_merchant_txn_ref_no").value;
			var txnamt = document.getElementById("nb_txn_amount").value;
			var customerId = document.getElementById("nb_customer_id").value;
			var custMobile = document.getElementById("nb_customer_mobile_no").value;
			var clientId = document.getElementById("nb_client_id").value;
			var returnurl = document.getElementById("nb_return_url").value;
			var txntime = document.getElementById("nb_timestamp").value;
			var transactionType = document.getElementById("nb_txn_type").value;
			var txncurrency = document.getElementById("nb_txn_currency").value;
			var customername = document.getElementById("nb_customer_name").value;
			var custEmailId = document.getElementById("nb_customer_email").value;
			var custAdd1 = document.getElementById("nb_customer_addr_line1").value;
			var custAdd2 = document.getElementById("nb_customer_addr_line2").value;
			var custCity = document.getElementById("nb_customer_city").value;
			var custState = document.getElementById("nb_customer_state").value;
			var custZipcode = document.getElementById("nb_customer_zipcode").value;
			var productDesc = document.getElementById("nb_product_description").value;
			var udf1 = document.getElementById("nb_udf1").value;
			var udf2 = document.getElementById("nb_udf2").value;
			var udf3 = document.getElementById("nb_udf3").value;
			var udf4 = document.getElementById("nb_udf4").value;
			var udf5 = document.getElementById("nb_udf5").value;
			var ipAddress = document.getElementById("nb_ip_address").value;
			var userAgent = document.getElementById("nb_useragent").value;
			var cardNumber = document.getElementById("nb_card_number").value;
			var cardName = document.getElementById("nb_card_name").value;
			var cardCVV = document.getElementById("nb_card_cvv").value;
			var cardExpMonth = document.getElementById("nb_card_expiry_month").value;
			var CardExpYear = document.getElementById("nb_card_expiry_year").value;
			var fundingSource = document.getElementById("nb_funding_source").value;
			var bankId = document.getElementById("nb_bank_id").options[document.getElementById("nb_bank_id").selectedIndex].value;
			//var bankName = document.getElementById("nb_bank_id").options[document.getElementById("nb_bank_id").selectedIndex].text;
