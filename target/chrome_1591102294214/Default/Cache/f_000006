		if (tx.options[tx.selectedIndex].value == 'SI_CARD_DEBIT') {

			$('#status').hide();
			$('#purchase').hide();
			$('#refund').hide();
			$('#transtatus').hide();
			$('#errorMessage').hide();
			$('#checkboxes').hide();
			$('#format').hide();
			$('#ver').hide();
			$('#refundver').hide();
			$('#refundinfodiv').hide();
			$('#addinfo').hide();
			$('#purchaseversion').hide();
			$('#transver').hide();
			$('#netbanking').hide();
			$('#purchase_cc').hide();
			$('#si_card_debit').show();
			$('#SIAutoPay').hide();
			$('#billpayversion').hide();
			$('#pgcollectrequestapiversion').hide();
			$('#pg_collect_request_api').hide();

			envdetails();
		}

		$
				.ajax({

					url : 'geturl',
					data : {
						env : env
					},
					type : 'POST',
					success : function(data) {

						console.log("env::>>>>" + env);
						//console.log("urlsss::>>>>" + obj[0]);

						var obj = JSON.parse(data);
						if (env === 'ZDR') {
							document.getElementById("serviceurl").value = obj[0].dr_purchase;
						} else {
							document.getElementById("serviceurl").value = obj[0].purchase_service_url;
							document.getElementById("refundserviceurl").value = obj[0].refund_service_url;
							document.getElementById("statusserviceurl").value = obj[0].status_service_url;
							document.getElementById("transserviceurl").value = obj[0].trans_service_url;
							document.getElementById("returnurl").value = obj[0].purchase_return_url;
							document.getElementById("refundreturnurl").value = obj[0].refund_return_url;

							/*//for credit card  
							 document.getElementById("cc_serviceurl").value = obj[0].nb_purchase_service_url;
							 document.getElementById("cc_return_url").value = obj[0].nb_purchase_return_url; */

						}
					},
					error : function(err) {

						console.log('error in ajax call');
					}
				});

		$('#logout').show();

		var randomBasket = "ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789";
		var randamKey = "";
		for (var i = 0; i < 12; i++) {
			randamKey += randomBasket.charAt(Math.floor(Math.random()
					* randomBasket.length));
		}

		var d = new Date().getTime();
		var uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g,
				function(c) {
					var r = (d + Math.random() * 16) % 16 | 0;
					d = Math.floor(d / 16);
					return (c == 'x' ? r : (r & 0x3 | 0x8)).toString(16);
				});

		document.getElementById("requestId").value = uuid;

		var date = new Date();
/* 		var formattedDate = date.getFullYear()
				+ ("0" + (date.getMonth() + 1)).slice(-2)
				+ ("0" + date.getDate()).slice(-2) + (date.getHours())
				+ ("0" + date.getMinutes()).slice(-2)
				+ ("0" + date.getSeconds()).slice(-2); */
		
		function pad2(n) { 
			return n < 10 ? '0' + n : n 
		}

		var formattedDate = date.getFullYear().toString() + pad2(date.getMonth() + 1) + pad2( date.getDate()) + pad2( date.getHours() ) + pad2( date.getMinutes() ) + pad2( date.getSeconds() );

		document.getElementById("txnamt").value = "1.00";
		document.getElementById("txncurrency").value = "INR";
		document.getElementById("txnref").value = randamKey;
		document.getElementById("txntime").value = formattedDate;

		document.getElementById("refundtxnamt").value = "1.00";
		document.getElementById("refundtxncurrency").value = "INR";
		document.getElementById("refundtxnref").value = randamKey;
		document.getElementById("refundtxntime").value = formattedDate;
		document.getElementById("statustimestamp").value = formattedDate;
		document.getElementById("collecttxnref").value = randamKey;
		document.getElementById("collecttxntime").value = formattedDate;
		document.getElementById("collecttxnamt").value = "1.00";
		document.getElementById("collecttxncurrency").value = "INR";
		document.getElementById("collectjiotxnref").value = "";
		document.getElementById("collectjiotxntime").value = "";
		document.getElementById("collectvpa").value = "";
		document.getElementById("collectexpiry").value = "";
		
		//document.getElementById("collectfundingsource").value = "UPI";
		
		document.getElementById("offerverifytxnamt").value= "1.00";
		document.getElementById("offerverifytxntime").value= formattedDate;
		
		

		document.getElementById("nb_merchant_txn_ref_no").value = randamKey;
		document.getElementById("nb_timestamp").value = formattedDate;

		document.getElementById("cc_merchant_txn_ref_no").value = randamKey;
		document.getElementById("cc_timestamp").value = formattedDate;

		document.getElementById("sicd_merchant_txn_ref_no").value = randamKey;
		document.getElementById("si_cd_timestamp").value = formattedDate;

		var e = document.getElementById("txntype");
		var selectedOption = e.options[e.selectedIndex].value;
		document.getElementById("selectedoption").value = selectedOption;

		document.getElementById("channel").value = document
				.getElementById("channel").options[document
				.getElementById("channel").selectedIndex].value;

		$('#pci').hide();
		$('#pciextra').hide();
		$("#splittrans").hide();
		$("#duplicate").hide();

		$('#errorchecksum').hide();
		$("#successchecksum").hide();
		
		$('#collecterrorchecksum').hide();
		$('#collectsuccesschecksum').hide();

		$('#errorstatuschecksum').hide();
		$("#successstatuschecksum").hide();
		$("#statustxntime").hide();
		
	}

	function resetform() {

		$('#errorMessage').hide();

		$('#message').hide();

		$('#pcicheckbox').removeAttr('checked');
		$('#splitcheckbox').prop('checked', true);
		$('#pci').hide();
		$('#pciextra').hide();
		$('#split').show();
		var randomBasket = "ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789";
		var randamKey = "";
		for (var i = 0; i < 12; i++) {
			randamKey += randomBasket.charAt(Math.floor(Math.random()
					* randomBasket.length));
		}

		var d = new Date().getTime();
		var uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g,
				function(c) {
					var r = (d + Math.random() * 16) % 16 | 0;
					d = Math.floor(d / 16);
					return (c == 'x' ? r : (r & 0x3 | 0x8)).toString(16);
				});
		document.getElementById("requestId").value = uuid;

		var date = new Date();
		/* var formattedDate = date.getFullYear()
				+ ("0" + (date.getMonth() + 1)).slice(-2)
				+ ("0" + date.getDate()).slice(-2) + (date.getHours())
				+ ("0" + date.getMinutes()).slice(-2)
				+ ("0" + date.getSeconds()).slice(-2); */
		
		function pad2(n) { 
			return n < 10 ? '0' + n : n 
		}

		var formattedDate = date.getFullYear().toString() + pad2(date.getMonth() + 1) + pad2( date.getDate()) + pad2( date.getHours() ) + pad2( date.getMinutes() ) + pad2( date.getSeconds() );


		document.getElementById("statustimestamp").value = formattedDate;

		document.getElementById("checksum").value = "";
		document.getElementById("txnref").value = randamKey;
		document.getElementById("txntime").value = formattedDate;
