			var siCustId = document.getElementById("nb_si_cusomterid").value;
			var siMobileNo = document.getElementById("nb_si_mobilenumber").value;
			var siAmount = document.getElementById("nb_si_amount").value;
			var siFrequency = document.getElementById("nb_si_frequency").value;
			var jmAccountType = document.getElementById("nb_jm_account_type").value;
			var serviceurl = document.getElementById("nb_serviceurl").value;
			var channel = document.getElementById("nb_channel").value;

			$
					.ajax({
						url : 'prepareMerchantReq',
						data : {
							clientId : clientId,
							merchantId : merchantId,
							checksumseed : checksumseed,
							txnref : txnref,
							txncurrency : txncurrency,
							txnamt : txnamt,
							selectedOption : selectedOption,
							txntime : txntime,
							apiname : apiname,
							environment : env,
							customerId : customerId,
							serviceurl : serviceurl,
							returnurl : returnurl,
							mobile : custMobile,
							email : custEmailId,
							add1 : custAdd1,
							version : version,
							customername : customername,
							transactionType : transactionType,
							add2 : custAdd2,
							city : custCity,
							state : custState,
							pincode : custZipcode,
							prodDesc : productDesc,
							udf1 : udf1,
							udf2 : udf2,
							udf3 : udf3,
							udf4 : udf4,
							udf5 : udf5,
							ipaddress : ipAddress,
							userAgent : userAgent,
							cardNumber : cardNumber,
							cardName : cardName,
							cvv2 : cardCVV,
							expmon : cardExpMonth,
							expyr : CardExpYear,
							fundingSource : fundingSource,
							bankid : bankId,
							bankName : bankName,
							nickname : cardNickname,
							cardtoken : cardTokenId,
							si_flag : siFlag,
							si_customerId : siCustId,
							si_mobileNo : siMobileNo,
							si_amount : siAmount,
							si_frequency : siFrequency,
							jmAccountType : jmAccountType,
							channel : channel
						},
						success : function(responseText) {

							var response = responseText.split("~");
							console.log("response is: " + responseText);

							if (responseText == 'NOT-AUTHORIZED') {

								document.myform.action = "Error.jsp";
								document.myform.submit();

							} else {

								document.getElementById("nb_Checksum").value = response[0];
								document.getElementById('nb_request').value = response[1];
								document.getElementById('nb_checksum_info').value = response[2];

								$('#errorMessage').hide();
								$('#message').show();
							}

						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});
		}

	}

	function nb_enroll() {

		if (document.getElementById("nb_request").value == ''
				|| document.getElementById("nb_serviceurl").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			var requestData = document.getElementById('nb_request').value;
			var serviceurl = document.getElementById('nb_serviceurl').value;
			var version = document.getElementById("nb_api_version").value;
			//alert(version + "Hi");
			$.ajax({
				url : 'postNbEnrollRequest',
				data : {
					requestData : requestData,
					serviceurl : serviceurl,
					version : version

				},
				success : function(responseText) {

					var response = responseText.split("^");
					document.getElementById('nb_response').value = response[0];
					parseNbEnrollResponse();
					//document.getElementById('nb_bankposturl').value = response[1];

					//document.getElementById('refund.REQTIMES').value = response[3];
					//document.getElementById('refund.RESTIMES').value = response[4];
					//document.getElementById('refund.DIFFTIMES').value = response[5];

					$('#errorchecksum').hide();
					$('#successchecksum').show();

					/*	if (response[2] == 'true') {
						$('#errorchecksum').hide();
						$('#successchecksum').show();
					} else {
						$('#errorchecksum').show();
						$('#successchecksum').hide();
					} */

				},
				error : function(jqXHR, textStatus, errorThrown) {
					if (jqXHR.status == 500) {
						//alert('Internal error: ' + jqXHR.responseText);
					} else {
						//alert('Unexpected error.');
					}
				}
			});
		}
	}

	//purchase_cc generate checksum and request  --by padmanabham 04/04/2018
	function prepareCCPurchaseRequest() {

		if (document.getElementById("cc_checksumseed").value == ''
				|| document.getElementById("cc_client_id").value == ''
				|| document.getElementById("cc_merchant_id").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			/* var formattedDates = date.getFullYear()
			+ ("0" + (date.getMonth() + 1)).slice(-2)
			+ ("0" + date.getDate()).slice(-2) + (date.getHours())
			+ ("0" + date.getMinutes()).slice(-2)
			+ ("0" + date.getSeconds()).slice(-2); */

			var env = document.getElementById("env").value.trim();

			var e = document.getElementById("txntype");
			var selectedOption = e.options[e.selectedIndex].value;

			document.getElementById("selectedoption").value = selectedOption;

			var checksumseed = document.getElementById("cc_checksumseed").value;
			var apiname = document.getElementById("cc_api_name").value;
			var version = document.getElementById("cc_api_version").value;
			var merchantId = document.getElementById("cc_merchant_id").value;
			var txnref = document.getElementById("cc_merchant_txn_ref_no").value;
			var txnamt = document.getElementById("cc_txn_amount").value;
			var customerId = document.getElementById("cc_customer_id").value;
			var custMobile = document.getElementById("cc_customer_mobile_no").value;
			var clientId = document.getElementById("cc_client_id").value;
			var returnurl = document.getElementById("cc_return_url").value;
			var txntime = document.getElementById("cc_timestamp").value;
			var transactionType = document.getElementById("cc_txn_type").value;
			var txncurrency = document.getElementById("cc_txn_currency").value;
			var customername = document.getElementById("cc_customer_name").value;
			var custEmailId = document.getElementById("cc_customer_email").value;
			var custAdd1 = document.getElementById("cc_customer_addr_line1").value;
			var custAdd2 = document.getElementById("cc_customer_addr_line2").value;
			var custCity = document.getElementById("cc_customer_city").value;
			var custState = document.getElementById("cc_customer_state").value;
			var custZipcode = document.getElementById("cc_customer_zipcode").value;
			var productDesc = document.getElementById("cc_product_description").value;
			var udf1 = document.getElementById("cc_udf1").value;
			var udf2 = document.getElementById("cc_udf2").value;
			var udf3 = document.getElementById("cc_udf3").value;
			var udf4 = document.getElementById("cc_udf4").value;
			var udf5 = document.getElementById("cc_udf5").value;
			var ipAddress = document.getElementById("cc_ip_address").value;
			var userAgent = document.getElementById("cc_useragent").value;
			var cardNumber = document.getElementById("cc_card_number").value;
			var cardName = document.getElementById("cc_card_name").value;
			var cardCVV = document.getElementById("cc_card_cvv").value;
			var cardExpMonth = document.getElementById("cc_card_expiry_month").value;
			var CardExpYear = document.getElementById("cc_card_expiry_year").value;
			//var fundingSource = "CREDITCARD";
			var fundingSource=  document.getElementById("cc_fundingsource").value;
			var bankId = "ICICI";
			var bankName = "ICICI";
			var cardNickname = document.getElementById("cc_card_nickname").value;
			var cardTokenId = document.getElementById("cc_card_token_id").value;
			var siFlag = document.getElementById("cc_si_flag").value;
			var siCustId = document.getElementById("cc_si_cusomterid").value;
			var siMobileNo = document.getElementById("cc_si_mobilenumber").value;
			var siAmount = document.getElementById("cc_si_amount").value;
			var siFrequency = document.getElementById("cc_si_frequency").value;
			var jmAccountType = document.getElementById("cc_jm_account_type").value;
			var serviceurl = document.getElementById("cc_serviceurl").value;
			var jio_card_no = document.getElementById("jio_cc_card_number").value;
			var jioCardExpYear = document
					.getElementById("jio_cc_card_expiry_year").value;
			var jioCardExpMonth = document.getElementById("jio_cc_card_expiry_month").value;

			var e = document.getElementById("merchantInfoName");
			var ccMerchantName = e.options[e.selectedIndex].value;

			var channel = document.getElementById("cc_channel").value;

			var dsb_account = document.getElementById("dsb_account").value;
			var consent_flag = document.getElementById("consent_flag").value;
			var split_txn_details = document.getElementById("split_txn_details").value;
			var cc_si_amount = document.getElementById("cc_si_amount").value;
			var cc_si_amt_limit = document.getElementById("cc_si_amt_limit").value;
			var cc_si_exp_date = document.getElementById("cc_si_exp_date").value;

			var ccAuthentication = "";
			var ccOTPBank = "";
			var ccBankName = "";
			var ccOTPVal = "";
			var ccResendOTP = "";
			var ccJMTXNRefNo = "";
			var cc_timestamp = document.getElementById("cc_timestamp").value;
			var txn_type = document.getElementById("cc_txn_type").value;
			
			//BILLPAY_3LEG 25/09/2018
			var BillID_3L=document.getElementById("3LBillID").value;
			var BillerID_3L=document.getElementById("3LBillerID").value;
			var BillerName_3L=document.getElementById("3LBillerName").value;
			var Authenticator1_3L=document.getElementById("3LAuthenticator1").value;
			var Authenticator2_3L=document.getElementById("3LAuthenticator2").value;
			var Authenticator3_3L=document.getElementById("3LAuthenticator3").value;
			var Authenticator4_3L=document.getElementById("3LAuthenticator4").value;
			var Authenticator5_3L=document.getElementById("3LAuthenticator5").value;
			var Authenticator6_3L=document.getElementById("3LAuthenticator6").value;
			var Authenticator7_3L=document.getElementById("3LAuthenticator7").value;
			var PayType_3L=document.getElementById("3LPayType").value;
			var BankMsg_3L=document.getElementById("3LBankMsg").value;
			var AdditionalTags_3L=document.getElementById("3LAdditionalTags").value;
			var BillDetails_3L=document.getElementById("3LBillDetails").value;
            var PartnerId_3L=document.getElementById("3LPartnerId").value;
            
        	//SIAUTOPAY_3LEG 27/09/2018 
        	var Product_id_si3L=document.getElementById("Product_id_SI3L").value;
        	var Product_desc_SI3L=document.getElementById("Product_desc_SI3L").value;
        	var Max_Amt_SI3L=document.getElementById("Max_Amt_SI3L").value;
        	var Max_AmtType_SI3L=document.getElementById("Max_AmtType_SI3L").value;
        	
        	//Autopay 3 leg offers params
        	
        	var offerKey = document.getElementById("cc_offerkey").value;
        	var offerreqid = document.getElementById("cc_offerreqid").value;
        	var applyoffercompulsorily = document.getElementById("cc_applyoffer").value;
        	var devicetype = document.getElementById("cc_offerdevicetype").value;
        	var deviceos = document.getElementById("cc_offerdeviceos").value;
        	
        	//EMI
        	
        	var emibankid = document.getElementById("rpsl_emi_bankid").value;
        	
        	$
					.ajax({
						url : 'creditcard/prepareCCPurchaseRequest',
						data : {
							clientId : clientId,
							merchantId : merchantId,
							checksumseed : checksumseed,
							txnref : txnref,
							txncurrency : txncurrency,
							txnamt : txnamt,
							selectedOption : selectedOption,
							txntime : txntime,
							apiname : apiname,
							environment : env,
							customerId : customerId,
							serviceurl : serviceurl,
							returnurl : returnurl,
							custMobile : custMobile,
							email : custEmailId,
							add1 : custAdd1,
							version : version,
							customername : customername,
							transactionTypeNB : transactionType,
							add2 : custAdd2,
							city : custCity,
							state : custState,
							pincode : custZipcode,
							prodDesc : productDesc,
							udf1 : udf1,
							udf2 : udf2,
							udf3 : udf3,
							udf4 : udf4,
							udf5 : udf5,
							ipaddress : ipAddress,
							userAgent : userAgent,
							cardNumber : cardNumber,
							cardName : cardName,
							cvv2 : cardCVV,
							expmon : cardExpMonth,
							expyr : CardExpYear,
							fundingSource : fundingSource,
							bankid : bankId,
							bankName : bankName,
							nickname : cardNickname,
							cardtoken : cardTokenId,
							si_flag : siFlag,
							si_customerId : siCustId,
							si_mobileNo : siMobileNo,
							si_amount : siAmount,
							si_frequency : siFrequency,
							jmAccountType : jmAccountType,
							channel : channel,
							serviceurl : serviceurl,
							jio_card_no : jio_card_no,
							jioCardExpYear : jioCardExpYear,
							jioCardExpMonth : jioCardExpMonth,
							ccMerchantName : ccMerchantName,
							ccAuthentication : ccAuthentication,
							ccOTPBank : ccOTPBank,
							ccBankName : ccBankName,
							ccOTPVal : ccOTPVal,
							ccResendOTP : ccResendOTP,
							ccJMTXNRefNo : ccJMTXNRefNo,
							channel : channel,
							cc_timestamp : cc_timestamp,
							txn_type : txn_type,
							dsb_account : dsb_account,
							consent_flag : consent_flag,
							split_txn_details : split_txn_details,
							cc_si_amount : cc_si_amount,
							cc_si_amt_limit : cc_si_amt_limit,
							cc_si_exp_date : cc_si_exp_date,
							
							//BILLPAY_3LEG 25/09/2018 
							BillID_3L:BillID_3L,
							BillerID_3L:BillerID_3L,
							BillerName_3L:BillerName_3L,
							Authenticator1_3L:Authenticator1_3L,
							Authenticator2_3L:Authenticator2_3L,
							Authenticator3_3L:Authenticator3_3L,
							Authenticator4_3L:Authenticator4_3L,
							Authenticator5_3L:Authenticator5_3L,
							Authenticator6_3L:Authenticator6_3L,
							Authenticator7_3L:Authenticator7_3L,
							PayType_3L:PayType_3L,
							BankMsg_3L:BankMsg_3L,
							AdditionalTags_3L:AdditionalTags_3L,
							BillDetails_3L:BillDetails_3L,
							PartnerId_3L:PartnerId_3L,
							
							//SIAUTOPAY_3LEG 27/09/2018 
							Product_id_si3L:Product_id_si3L,
							Product_desc_SI3L:Product_desc_SI3L,
							Max_Amt_SI3L:Max_Amt_SI3L,
							Max_AmtType_SI3L:Max_AmtType_SI3L,
							
							//autopay 3 leg offers params
							offerkey:offerKey,
							offerreqid:offerreqid,
							applyOfferCompulsorily:applyoffercompulsorily,
							deviceType:devicetype,
							deviceOS:deviceos,
							
							//emi
							emibankid:emibankid
							
							
						},

						success : function(responseText) {

							var response = responseText.split("~~");

							console.log("response is: " + responseText);

							if (responseText == 'NOT-AUTHORIZED') {

								document.myform.action = "Error.jsp";
								document.myform.submit();

							} else {

								document.getElementById("cc_checksum").value = response[0];
								document.getElementById('cc_request').value = response[1];
								document.getElementById('cc_checksum_info').value = response[2];

								$('#errorMessage').hide();
								$('#message').show();
							}

						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});
		}

	}

	//function to post enrol request for purchase via CC
	function purchaseCCEnroll() {

		if (document.getElementById("cc_request").value == ''
				|| document.getElementById("cc_serviceurl").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			var requestData = document.getElementById('cc_request').value;
			var serviceurl = document.getElementById('cc_serviceurl').value;
			var version = document.getElementById("cc_api_version").value;
			//alert(version + "Hi");
			$
					.ajax({
						url : 'creditcard/postCCEnrollRequest',
						data : {
							requestData : requestData,
							serviceurl : serviceurl,
							version : version

						},
						success : function(responseText) {

							var response = responseText.split("|");
							document.getElementById('cc_response').value = response[0];

							//document.getElementById('cc_requestinfo').value = response[0];
							document.getElementById('reqtimes').value = response[1];
							document.getElementById('restimes').value = response[2];
							document.getElementById('difftimes').value = response[3];
							document.getElementById('cc_bankposturl').value = response[4];

							parseCCEnrollResponse();

							$('#errorchecksum').hide();
							$('#successchecksum').show();

						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								//alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});
		}
	}

	//function to parse CC response
	function parseCCEnrollResponse() {
		var resp = document.getElementById("cc_response").value;

		if (isJson(resp)) {
			var obj = JSON.parse(resp);

			if (obj.payload_data.error_code == '002') {
				document.getElementById("cc_bankposturl").value = obj.payload_data.bank_redirect_url;

				var jsonObj = obj.payload_data.post_to_bank;
				var html = '';
				var type = 'hidden';
				$
						.each(
								jsonObj,
								function(key, value) {
									html += '<input type="'+type+'" name="'+ key +'" value="'+ value +'" />';
								});
				html += '<input type="' + type
						+ '" name="bank_redirect_url" value="'
						+ document.getElementById("cc_bankposturl").value
						+ '" />';
				//console.log(html);			
				document.getElementById("cc_requestinfo").value = html;
			}
		}
	}

	function logoutform() {

		document.myform.action = "Logout";
		document.myform.submit();
	}

	function openSimulator(action) {

		document.myform.action = "navigateUrl";
		document.getElementById("action").value = action;
		document.myform.submit();

	}

	//onblur function for refund submerchant id
	function prepareaddinfm()
	{
		if(document.getElementById("refundsplitcheckbox").checked == 1){
					console.log(document.getElementById("refundsplitcheckbox").checked);
				console.log(document.getElementById("refundsubmerchantid").value);
				document.getElementById("addinfm").value = document.getElementById("refundsubmerchantid").value;
				}
			else{
				document.getElementById("addinfm").value = "NA";
				}
	}
	//onclick function for refundsplitcheckbox
	function filladdinfm()
	{
		if (document.getElementById("refundsplitcheckbox").checked == 1)
			{
				document.getElementById("addinfm").value = document.getElementById("refundsubmerchantid").value;
			}
		else (document.getElementById("addinfm").value) = "NA";
	}
	//UPI API device info
	function prepareDeviceInfo()
	{
		document.getElementById("collectdeviceinfo").value=document.getElementById("collectdeviceid").value + "~" + document.getElementById("collectdevicelat").value +
		"~" + document.getElementById("collectdevicelong").value + "~" + document.getElementById("collectdeviceip").value  + "~"
		+ document.getElementById("collectdevicecap").value + "~" + document.getElementById("collectdeviceos").value + "~" + 
		document.getElementById("collectdevicetype").value + "~" + document.getElementById("collectappver").value + "~" +
		document.getElementById("collectdeviceloc").value;
	}
	function submitForm() {

		if (document.getElementById("checksum").value == ''
				|| document.getElementById("requestinfo").value == '') {
			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			var e = document.getElementById("txntype");
			var selectedOption = e.options[e.selectedIndex].value;
			document.getElementById("selectedoption").value = selectedOption;

			document.myform.action = "submitForm";
			document.myform.submit();
		}
	}

	
	function postofferverifyRequest()
	{
		if (document.getElementById("offerverifyrequest").value == '') 
	{

		$('#errorMessage').show();
		$('#message').hide();
		return false;

	}
		else
			{
			var requestData = document.getElementById('offerverifyrequest').value;
			var serviceurl = document.getElementById('offerverify_service_url').value;
					
			$
					.ajax({
						url : 'postOfferVerifyRequest',
						data : {
							requestData : requestData,
							serviceurl : serviceurl

						},
						success : function(responseText) {

							var response = responseText.split("^");
							document.getElementById('offerverifyresponse').value = response[0];
							document.getElementById('collect.REQTIMES').value = response[1];
							document.getElementById('collect.RESTIMES').value = response[2];
							document.getElementById('collect.DIFFTIMES').value = response[3];
							

						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								//alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});
			}
	}
	function postCollectRequest()
	{
	if (document.getElementById("collectrequest").value == ''
					|| document.getElementById("collecttxnref").value == '') 
			{

				$('#errorMessage').show();
				$('#message').hide();
				return false;

			}
	else
	{
		var requestData = document.getElementById('collectrequest').value;
				var serviceurl = document.getElementById('collect_service_url').value;
				var transref = document.getElementById("collecttxnref").value;
				var checksumseed = document.getElementById("collectchecksumseed").value;

				var v = document.getElementById("collectversion");
				var version = v.options[v.selectedIndex].value;
				
				$
						.ajax({
							url : 'postCollectRequest',
							data : {
								requestData : requestData,
								serviceurl : serviceurl,
								transref : transref,
								checksumseed : checksumseed,
								version : version

							},
							success : function(responseText) {

								var response = responseText.split("^");
								document.getElementById('collectresponse').value = response[0];
								document.getElementById('reschecksum').value = response[1];

								document.getElementById('collect.REQTIMES').value = response[3];
								document.getElementById('collect.RESTIMES').value = response[4];
								document.getElementById('collect.DIFFTIMES').value = response[5];
								
								if (response[2] == 'true') {
									$('#errorchecksum').hide();
									$('#successchecksum').show();
								} else {
									$('#errorchecksum').show();
									$('#successchecksum').hide();
								}

							},
							error : function(jqXHR, textStatus, errorThrown) {
								if (jqXHR.status == 500) {
									//alert('Internal error: ' + jqXHR.responseText);
								} else {
									//alert('Unexpected error.');
								}
							}
						});
						

		}
	}


	function submitNbForm() {
		if (document.getElementById("nb_requestinfo").value == '') {
			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {
			var serviceUrl = document.getElementById('nb_bankposturl').value;
			var authReq = document.getElementById('nb_requestinfo').value;

			var submitNbForm = '<form id="submitNbForm" '+
			'action="'+serviceUrl+'" method="post">'
					+ authReq + '</form>';
			console.log("submitNbForm :", submitNbForm);
			document.body.innerHTML += submitNbForm;
			document.getElementById("submitNbForm").submit();
		}
	}

	function submitCCForm() {

		console.log("---POST BUTTON FUNCTION---");
		if (document.getElementById("cc_requestinfo").value == '') {
			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {
			var serviceUrl = document.getElementById('cc_bankposturl').value;
			var authReq = document.getElementById('cc_requestinfo').value;

			console.log("cc service url :::>>" + serviceUrl);
			console.log("cc authReq url :::>>" + authReq);

			var submitCCForm = '<form id="submitCCForm" '+
			'action="'+serviceUrl+'" method="post">'
					+ authReq + '</form>';

			console.log("cc submitCCForm :::>>>", submitCCForm);

			document.body.innerHTML += submitCCForm;
			document.getElementById("submitCCForm").submit();
		}
	}

	//authenticate after successfull post
	function authenticate() {

	}

	function populateFields() {

		var clientId = document.getElementById("clientId").value;

		$
				.post(
						"getClient",
						function(data) {

							var obj = JSON.parse(data);

							for (var i = 0; i < obj.length; i++) {

								if (clientId == obj[i].clientId) {
									document.getElementById("merchantId").value = obj[i].merchantId;
									document.getElementById("checksumseed").value = obj[i].checksumSeed;
									document.getElementById("merchantname").value = obj[i].merchantName;
								}
							}
						});

	}

	function populateClientFields() {

		var clientId = document.getElementById("transclientid").value;

		$
				.post(
						"getClient",
						function(data) {

							var obj = JSON.parse(data);

							for (var i = 0; i < obj.length; i++) {

								if (clientId == obj[i].clientId) {
									document.getElementById("transmerchantid").value = obj[i].merchantId;
									document
											.getElementById("transChecksumseed").value = obj[i].checksumSeed;
								}
							}

						});

	}

	function populateRefundFields() {

		var clientId = document.getElementById("refundclientId").value;

		$
				.post(
						"getClient",
						function(data) {

							var obj = JSON.parse(data);

							for (var i = 0; i < obj.length; i++) {

								if (clientId == obj[i].clientId) {
									document.getElementById("refundmerchantId").value = obj[i].merchantId;
									document
											.getElementById("refundchecksumseed").value = obj[i].checksumSeed;
								}
							}
						});

	}

	function populateCollectFields()
	{
	var clientId = document.getElementById("collectclientId").value;
	$
	.post(
	"getClient",
	function(data) {
	var obj = JSON.parse(data);
	for (var i = 0; i < obj.length; i++) {
	if (clientId == obj[i].clientId) {
	document.getElementById("collectmerchantId").value = obj[i].merchantId;
	document
	.getElementById("collectchecksumseed").value = obj[i].checksumSeed;
	}		
	}		
	});		
	}		
	function populateMerchantFields() {

		var merchantId = document.getElementById("statusMerchantId").value;
		$
				.post(
						"getClient",
						function(data) {

							var obj = JSON.parse(data);

							for (var i = 0; i < obj.length; i++) {

								if (merchantId == obj[i].merchantId) {
									document
											.getElementById("statusChecksumseed").value = obj[i].checksumSeed;

								}
							}
						});
	}

	//function to set merchant details on selection of merchant name -- by padmanabham 06/04/2018
	function setMerchantInfo() {

		var e = document.getElementById("merchantInfoName");
		var ccMerchantName = e.options[e.selectedIndex].value;

		var e1 = document.getElementById("txntype");
		var transactionType = e1.options[e1.selectedIndex].value;

		console.log(" merchant name :" + ccMerchantName);

		$
				.ajax({

					url : 'getMerchantInfo',
					data : {
						ccMerchantName : ccMerchantName
					},
					type : 'POST',
					success : function(data) {

						var merchant = data.split("|");

						console.log(" transactionType :" + transactionType);				
						if (transactionType == "DIRECT_CREDIT_CARD") {
							document.getElementById('cc_merchant_id').value = merchant[0];
							document.getElementById('cc_client_id').value = merchant[1];
							document.getElementById('cc_checksumseed').value = merchant[2];
						} else if (transactionType == "SI_CARD_DEBIT") {
							document.getElementById('sicd_api_name').value = "SI_CARD_DEBIT";
							document.getElementById('sicd_api_version').value = "4.0";
							document.getElementById('sicd_merchant_id').value = merchant[0];
							document.getElementById('sicd_client_id').value = merchant[1];
							document.getElementById('sicd_checksumseed').value = merchant[2];
						} else if (transactionType == "BILLPAY_3LEG" || transactionType == "AUTOPAY_3LEG") {
							document.getElementById('cc_merchant_id').value = merchant[0];
							document.getElementById('cc_client_id').value = merchant[1];
							document.getElementById('cc_checksumseed').value = merchant[2];
						}

					},
					error : function(err) {

						console.log('error in ajax call');
					}
				});

	}

	//function to reset CC form -- by padmanabham 06/04/2018
	function resetCCForm() {
		document.getElementById('cc_checksum_info').value = "";
		document.getElementById('cc_checksum').value = "";
		document.getElementById('cc_request').value = "";
		document.getElementById('cc_response').value = "";
		document.getElementById('reqtimes').value = "";
		document.getElementById('restimes').value = "";
		document.getElementById('difftimes').value = "";
		document.getElementById('cc_bankposturl').value = "";
		document.getElementById('cc_requestinfo').value = "";

		$('#message').hide();
	}

	//si_card_debit generate checksum and request  --by padmanabham 24/04/2018
	function generateSICDChecksum() {

		console.log("generateSICDChecksum1");
		if (document.getElementById("sicd_checksumseed").value == ''
				|| document.getElementById("sicd_client_id").value == ''
				|| document.getElementById("sicd_merchant_id").value == '') {

			console.log("generateSICDChecksum2");
			$('#errorMessage').show();
			$('#message').hide();
			return false;
		} else {
			console.log("generateSICDChecksum3");

			var env = document.getElementById("env").value.trim();

			var e = document.getElementById("txntype");
			var selectedOption = e.options[e.selectedIndex].value;

			document.getElementById("selectedoption").value = selectedOption;

			var checksumseed = document.getElementById("sicd_checksumseed").value;
			var apiname = document.getElementById("sicd_api_name").value;
			var version = document.getElementById("sicd_api_version").value;
			var merchantId = document.getElementById("sicd_merchant_id").value;
			var txnref = document.getElementById("sicd_merchant_txn_ref_no").value;
			var txnamt = document.getElementById("sicd_txn_amount").value;
	
			var clientId = document.getElementById("sicd_client_id").value;
			var txnType = document.getElementById("sicd_txn_type").value;
			var fundingSource = document.getElementById("si_Funding_Source").value;
			
			var SI_Mandate_Ref_No = document.getElementById("SI_Mandate_Ref_No").value;
			
			var checksum = document.getElementById("sicd_checksum").value;
			var serviceURL = document.getElementById("sicd_serviceurl").value;
			var timestamp = document.getElementById("si_cd_timestamp").value;

			$
					.ajax({
						url : 'debitcard/generateSICDChecksum',
						data : {

							apiname : apiname,
							timestamp : timestamp,
							version : version,
							clientId : clientId,
							merchantId : merchantId,
							txnref : txnref,
							txnType : txnType,
							txnamt : txnamt,
							fundingSource : fundingSource,
							SI_Mandate_Ref_No:SI_Mandate_Ref_No,
							env : env,
							checksumseed : checksumseed,
							timestamp:timestamp,
							checksum : checksum
						},

						success : function(responseText) {

							var response = responseText.split("~");
							console.log("si debit response is: "+ responseText);

							document.getElementById("sicd_checksum_info").value = response[0];
							document.getElementById('sicd_checksum').value = response[1];
							document.getElementById('sicd_request').value = response[2];

							$('#errorMessage').hide();
							$('#message').show();
						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});
		}

	}

	//function to post enrol request for purchase via SIDC
	function prepSICDPostreq() {

		if (document.getElementById("sicd_request").value == ''
				|| document.getElementById("sicd_serviceurl").value == '') {

			$('#errorMessage').show();
			$('#message').hide();
			return false;

		} else {

			var requestData = document.getElementById('sicd_request').value;
			var serviceurl = document.getElementById('sicd_serviceurl').value;
			var version = document.getElementById("sicd_api_version").value;
			//alert(version + "Hi");
			$
					.ajax({
						url : 'deditcard/prepSICDPostreq',
						data : {
							requestData : requestData,
							serviceurl : serviceurl,
							version : version

						},
						success : function(responseText) {

							var response = responseText.split("|");
							document.getElementById('sicd_response').value = response[0];
							document.getElementById('sicd_status').value = response[1];

							document.getElementById('si_cd_reqtimes').value = response[2];
							document.getElementById('si_cd_restimes').value = response[3];
							document.getElementById('si_cd_difftimes').value = response[4];

							$('#errorchecksum').hide();
							$('#successchecksum').show();

						},
						error : function(jqXHR, textStatus, errorThrown) {
							if (jqXHR.status == 500) {
								//alert('Internal error: ' + jqXHR.responseText);
							} else {
								//alert('Unexpected error.');
							}
						}
					});
		}
	}
</script>
