								$('#transver').hide();
								//$('#version').hide();//MH9754
								$('#netbanking').hide();
								$('#si_card_debit').hide();
								document.getElementById("cc_txn_type").value = document.getElementById("txntype").options[document
										.getElementById("txntype").selectedIndex].value;
								document.getElementById("cc_api_name").value = document
										.getElementById("txntype").options[document
										.getElementById("txntype").selectedIndex].value;
								$('#SIAutoPay').hide();
								$('#BillPay').hide();
								$('#billpayversion').hide();
								$('#pg_collect_request_api').hide();
								$('#pgcollectrequestapiversion').hide();
								$('#offerverifymerchant').hide();
							}

							if (this.options[this.selectedIndex].value == 'REFUND') {
								$('#refund').show();
								$('#purchase').hide();
								$('#status').hide();
								$('#transtatus').hide();
