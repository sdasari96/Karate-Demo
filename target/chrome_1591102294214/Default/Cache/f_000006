								$('#errorMessage').hide();
								$('#checkboxes').hide();
								$('#format').show();
								$('#ver').hide();
								$('#refundver').hide();
								$('#refundinfodiv').hide();
								$('#addinfo').hide();
								$('#purchaseversion').hide();
								$('#transver').show();
