function check3Sequence(h,k){for(var x=h.split(""),u=0,r=k+1,B=r,H=0;H<x.length;H++){var A=x[H].charCodeAt(0);65<=A&&90>=A&&(A+=32);B=u+1==A?B-1:r-1;if(0===B)return!1;u=A}return!0};